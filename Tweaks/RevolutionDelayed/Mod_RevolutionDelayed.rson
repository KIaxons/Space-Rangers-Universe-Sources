          "#":  8,
          "Total.Lines":  230,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if(pirates_begin_turn > CurTurn())",
              "{",
              "    for(i = 0; i < GalaxyStars(); i = i + 1)",
              "    {",
              "        cur_star = GalaxyStar(i);",
              "        if(StarOwner(cur_star) == 2) continue;",
              "",
              "        //Уничтожаем все вылетевшие за пределы скрытого сектора пиратские корабли в не пиратских системах",
              "        for(j = 0; j < StarShips(cur_star); j = j + 1)",
              "        {",
              "            cur_ship = StarShips(cur_star, j);",
              "            if(ShipOwner(cur_ship) != PirateClan) continue;",
              "",
              "            TransferShip(cur_ship, StarPlanets(GalaxyStar(People), 0)); //На Меркурий",
              "            OrderNone(cur_ship);",
              "            ShipDestroy(cur_ship, 1);",
              "        }",
              "    }",
              "}",
              "else",
              "{",
              "    if(!pirates_captured_first_star)",
              "    {",
              "        if(!pirates_advance_started)",
              "        {",
              "            dword tortugatz = GalaxyStar(70);",
              "            int stars_to_attack = PortionInDiapason(GalaxyDiffLevels(0), 50, 500, 4, 12) + Rnd(0, 2);",
              "            unknown stars_to_attack_arr = newarray(1);",
              "            ArrayDelete(stars_to_attack_arr, 0);",
              "            for(i = 0; i < GalaxyStars(); i = i + 1)",
              "            {",
              "                cur_star = StarNearbyStars(tortugatz, i);",
              "                if(cur_star == GalaxyStar(71)) continue; //Пропускаем Нифигац",
              "",
              "                ArrayAdd(stars_to_attack_arr, cur_star);",
              "                j = j + 1;",
              "                if(j == stars_to_attack) break;",
              "            }",
              "",
              "            int dominions_limit = PortionInDiapason(GalaxyDiffLevels(0), 50, 500, 0, 5);",
              "            for(i = 0; i < ArrayDim(stars_to_attack_arr); i = i + 1)",
              "            {",
              "                cur_star = stars_to_attack_arr[i];",
              "",
              "                int attackers = PortionInDiapason(GalaxyDiffLevels(0), 50, 500, 12, 45) + Rnd(0, 5);",
              "                //Создаём нападающих пиратов",
              "                for(j = 1; j <= attackers; j = j + 1)",
              "                {",
              "                    cur_ship = BuyWarrior(PlanetPirateClan(), 500);",
              "                    ShipRefit(cur_ship);",
              "                    ShipRefit(cur_ship);",
              "                    ShipRefit(cur_ship);",
              "                    ShipImproveItems(cur_ship, RndObject(5, 10, cur_ship));",
              "                    RaisePirateRank(cur_ship, min(GetShipPirateRank(cur_ship), 3));",
              "                    ShipJoin(AttackersGroup, cur_ship, '', Id(cur_star));",
              "                }",
              "",
              "                if(dominions_limit)",
              "                {",
              "                    if(RndObject(1, 100, cur_star) <= 60)",
              "                    {",
              "                        dword cur_ruin = BuildRuins(cur_star, t_CB);",
              "",
              "                        //Создаём гарнизон доминиона",
              "                        attackers = RndObject(3, 5, cur_ruin);",
              "                        for(j = 1; j <= attackers; j = j + 1)",
              "                        {",
              "                            cur_ship = BuyWarrior(PlanetPirateClan(), 500);",
              "                            ShipRefit(cur_ship);",
              "                            ShipRefit(cur_ship);",
              "                            ShipRefit(cur_ship);",
              "                            ShipImproveItems(cur_ship, RndObject(5, 10, cur_ship));",
              "                            RaisePirateRank(cur_ship, min(GetShipPirateRank(cur_ship), 3));",
              "                            TransferShip(cur_ship, cur_ruin);",
              "                        }",
              "",
              "                        MakeShipEnterStar(cur_ruin, cur_star, tortugatz, 7);",
              "                        OrderTeleport(cur_ruin, cur_star, CoordX(cur_ruin), CoordY(cur_ruin), 1, 7);",
              "                        dominions_limit = dominions_limit - 1;",
              "                    }",
              "                }",
              "            }",
              "",
              "            //Разрешаем Роджерии спавнить пиратов на ПБ в Коалиционных системах",
              "            if(!CurrentMods('ShusRangers\\ShuPirates')) PlanetExtraFlags(PlanetPirateClan(), 0);",
              "",
              "            pirates_advance_started = 1;",
              "        }",
              "",
              "        for(i = GalaxyEvents() - 1; i >= 0; i = i - 1)",
              "        {",
              "            if(GalaxyEventDate(i) < CurTurn()) break;",
              "            if(GalaxyEventType(i) != 'RuinsCreated') continue;",
              "",
              "            str_type = GalaxyEventsTextData(i, 1);",
              "            Format(str_type, '_licensed', '', '');",
              "            if(str_type == '')",
              "            {",
              "                if(GalaxyEventData(i, 0) == t_PB)",
              "                {",
              "                    if(control_ruins_arr[0]) ArrayAdd(control_ruins_arr, GalaxyEventData(i, 1));",
              "                    else control_ruins_arr[0] = GalaxyEventData(i, 1);",
              "                }",
              "            }",
              "            else if(str_type == 'MB' || str_type == 'Manaan' || str_type == 'MercHQ' || str_type == 'PlayerMercHQ')",
              "            {",
              "                if(control_ruins_arr[0]) ArrayAdd(control_ruins_arr, GalaxyEventData(i, 1));",
              "                else control_ruins_arr[0] = GalaxyEventData(i, 1);",
              "            }",
              "        }",
              "",
              "        if(control_ruins_arr[0])",
              "        {",
              "            tint1 = ArrayDim(control_ruins_arr);",
              "            for(i = 0; i < tint1; i = i + 1)",
              "            {",
              "                cur_ruin = IdToShip(control_ruins_arr(i));",
              "                if(!cur_ruin)",
              "                {",
              "                    if(tint1 > 1) ArrayDelete(control_ruins_arr, i);",
              "                    else control_ruins_arr[0] = 0;",
              "                    continue;",
              "                }",
              "",
              "                tint2 = ShopItems(cur_ruin);",
              "                for(j = 0; j < tint2; j = j + 1)",
              "                {",
              "                    dword cur_item = ShopItems(cur_ruin, j);",
              "                    if(ItemOwner(cur_item) != PirateClan) continue;",
              "",
              "                    if(ItemType(cur_item) == t_Hull)",
              "                    {",
              "                        FreeItem(GetItemFromShop(cur_ruin, i));",
              "                        continue;",
              "                    }",
              "",
              "                    if(~EqSpecial(cur_item)) SpecialToEquipment(-1, cur_item);",
              "                    if(~EqModule(cur_item)) ModuleToEquipment(-1, cur_item);",
              "",
              "                    //Цена малокской оборудки ближе всего к пиратской",
              "                    if(RndObject(0, 2, cur_ruin)) ItemOwner(cur_item, Maloc);",
              "                    else ItemOwner(cur_item, People);",
              "                }",
              "            }",
              "        }",
              "",
              "        for(i = 0; i < GalaxyStars(); i = i + 1)",
              "        {",
              "            cur_star = GalaxyStar(i);",
              "            if(StarOwner(cur_star) != 2) continue;",
              "            //Кроме скрытого сектора",
              "            if(StarToCon(cur_star) == StarToCon(PlanetToStar(PlanetPirateClan()))) continue;",
              "",
              "            pirates_captured_first_star = CurTurn();",
              "        }",
              "    }",
              "    //Возвращаем пиратские кнопки в инфоцентры",
              "    else",
              "    {",
              "        InterfacePos('Info', 'M01Maloc', 0, 0, 1);",
              "        InterfacePos('Info', 'M01Peleng', 0, 0, 1);",
              "        InterfacePos('Info', 'M01People', 0, 0, 1);",
              "        InterfacePos('Info', 'M01Fei', 0, 0, 1);",
              "        InterfacePos('Info', 'M01Gaal', 0, 0, 1);",
