          "Code.Type":  "Init",
          "Code":          
          [
              "if(CurTurn() > 301) throw 'Mod RevolutionDelayed was enabled after start of the game.';",
              "pirates_begin_turn = 301 + PortionInDiapason(GalaxyDiffLevels(0), 500, 50, 182, 1825) + Rnd(-60, 120);",
              "//pirates_items_availability_turn = pirates_begin_turn + 90;",
              "//Для отладки",
              "Ether(7, '', GameDateTxtByTurn(pirates_begin_turn));",
              "",
              "//Запрещаем Роджерии спавнить пиратов на ПБ в Коалиционных системах",
              "PlanetExtraFlags(PlanetPirateClan(), PlanetExtraFlags(PlanetPirateClan()) | 2);",
              "",
              "unknown control_ruins_arr = newarray(1);",
              "ArrayDelete(control_ruins_arr, 0);",
              "for(int i = 0; i < GalaxyStars(); i = i + 1)",
              "{",
              "    dword cur_star = GalaxyStar(i);",
              "    if(StarOwner(cur_star) != 2) continue;",
              "    //Кроме скрытого сектора",
              "    if(StarToCon(cur_star) == StarToCon(PlanetToStar(PlanetPirateClan()))) continue;",
              "",
              "    //Уничтожаем все пиратские корабли и доминионы в пиратских системах",
              "    tint1 = 0;",
              "    for(int j = 0; j < StarShips(cur_star); j = j + 1)",
              "    {",
              "        dword cur_ship = StarShips(cur_star, j);",
              "",
              "        //Составляем массив станций, на которых будет каждый ход проверять оборудку в магазине",
              "        str str_type = ShipType(cur_ship);",
              "        Format(str_type, '_licensed', '', '');",
              "        if(str_type == 'PB' || str_type == 'MB' || str_type == 'Manaan' || str_type == 'MercHQ' || str_type == 'PlayerMercHQ')",
              "        {",
              "            if(ShipOwner(cur_ship) != PirateClan)",
              "            {",
              "                ArrayAdd(control_ruins_arr, Id(cur_ship));",
              "                tint1 = tint1 + 1;",
              "                continue;",
              "            }",
              "            else",
              "            {",
              "                OrderNone(cur_ship);",
              "                OrderLock(cur_ship, 1);",
              "                ShipDestroy(cur_ship, 1);",
              "            }",
              "        }",
              "",
              "        if(ShipOwner(cur_ship) != PirateClan) continue;",
              "        OrderNone(cur_ship);",
              "        OrderLock(cur_ship, 1);",
              "        ShipDestroy(cur_ship, 1);",
              "    }",
              "    if(!tint1) ArrayAdd(control_ruins_arr, 0);",
              "",
              "    //Перебираем все пиратские планеты и возвращаем их на сторону Коалиции,",
              "    //а также докупаем на них военных, чтобы те обеспечили захват системы",
              "    for(j = 0; j < StarPlanets(cur_star); j = j + 1)",
              "    {",
              "        dword cur_planet = StarPlanets(cur_star, j);",
              "        if(PlanetOwner(cur_planet) == None) continue;",
              "        PlanetOwner(cur_planet, PlanetRace(cur_planet));",
              "        BuyWarrior(cur_planet);",
              "    }",
              "}",
              "",
              "//Скрываем пиратские кнопки в инфоцентрах",
              "InterfacePos('Info', 'M01Maloc', 0, 0, 1);",
              "InterfacePos('Info', 'M01Peleng', 4, 0, 1);",
