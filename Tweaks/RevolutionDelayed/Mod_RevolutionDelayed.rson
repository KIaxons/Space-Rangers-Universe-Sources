          [
              "dword cur_ruin = GetShipRuins(Player());",
              "if(cur_ruin)",
              "{",
              "    if(pirates_begin_turn > CurTurn())",
              "    {",
              "        if(ShipType(cur_ruin) == 'RC')",
              "        {",
              "            //Блочим вопрос игрока \"Как я могу помочь Коалиции в борьбе с пиратами?\"",
