              "                //Исключаем бонусы, неподходящие для текущего оборудования/оружия",
              "                str equipOn=CT('MicroModuls.'+numInCfg+'.Equipments');",
              "                if(equipOn != 'Any' && equipOn != '')",
              "                {",
              "                    if(type != 'Weapon')",
