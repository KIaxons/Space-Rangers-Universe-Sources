              "    int highest_value;",
              "    str highest_value_name;",
              "    str temp_w_type = 'None';",
              "    str end_w_type = 'None';",
              "    int acryn_param = 0;",
              "    int best_MM = -1;",
              "    int best_MM_end = -1;",
