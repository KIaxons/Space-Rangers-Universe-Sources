              "",
              "    int highest_gun4 = 0;",
              "    str highest_gun4_name = 'None';",
              "    int gun4_best_MM = -1;",
              "",
              "    int highest_gun5 = 0;",
              "    str highest_gun5_name = 'None';",
              "    int gun5_best_MM = -1;",
              "",
