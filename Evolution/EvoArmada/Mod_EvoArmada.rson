              "//Функция для спавна корветов",
              "function SpawnCorvette(dword ship, dword planet, int capital)",
              "{",
              "    if(!ship)",
              "    {",
              "        ship = BuyPirate(planet, capital);",
              "        ShipSubType(ship, 2);",
              "    }",
              "    ShipType(ship, 'PirateCorvette');",
              "",
              "    dword item = ShipItems(ship, 0);",
