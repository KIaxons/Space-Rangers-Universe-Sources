              "function g_KellerExchangeDialogCall(int skip)",
              "{",
              "    //Сразу звоним Келлеру",
              "    if((DiggerRevelationStatus & 8))",
              "    {",
              "        if(!skip)",
              "        {",
              "            if(!GetProgramm(progIntercom))",
              "            {",
              "                tstr1 = CT('Script.Mod_ShuDomiks.MainDiggerDialog.NoIntercom' + Rnd(1, 3, sqr(CurTurn())));",
              "                Sound('Sound.Talk');",
              "                MessageBox(tstr1);",
              "                exit;",
              "            }",
              "",
              "            tflt1 = RoundTo(CybercoinsCurExchangeRateMod() * KellerExchange['KellerCallFee'], 0.1, 2);",
              "            if(g_CountCybercoins() < tflt1) //У игрока недостаточно киберкойнов для вызова Келлера",
              "            {",
              "                Sound('Sound.Talk');",
              "                MessageBox(Format(CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchangeCallNotEnough'), '<Cybercoins'+'>', FloatToString(tflt1, 1)));",
              "                SetProgramm(progIntercom, GetProgramm(progIntercom) - 1); //Типо потратили на декодировку сообщения от \"секретаря\"",
              "                exit;",
              "            }",
              "",
              "            if( CurTurn() < KellerExchange['NextExchangeTurn'] ||",
              "                GetVariableType(KellerExchange['ChoosedMaterialsToBuy', 0]) ||",
              "                GetVariableType(KellerExchange['ChoosedMaterialsToSell', 0]) ) //Ещё не прошло достаточно времени с момента прошлого обмена с Келлером",
              "            {",
              "                Sound('Sound.Talk');",
              "                MessageBox(Format(CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchangeCallAfterTradeDelay'), '<Date'+'>', GameDateTxtByTurn(KellerExchange['NextExchangeTurn'])));",
              "                SetProgramm(progIntercom, GetProgramm(progIntercom) - 1); //Типо потратили на декодировку сообщения от \"секретаря\"",
              "                exit;",
              "            }",
              "",
              "            //Запрашиваем подтверждение перед вызовом Келлера",
              "            Sound('Sound.Talk');",
              "            if(!MessageBoxYesNo(Format(CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchangeCallConfirmation'), '<Cybercoins'+'>', FloatToString(tflt1, 1))))",
              "            {",
              "                SetProgramm(progIntercom, GetProgramm(progIntercom) - 1); //Типо потратили на декодировку сообщения от \"секретаря\"",
              "                exit;",
              "            }",
              "            else",
              "            {",
              "                //Списываем со счёта диггеров игрока tflt1 киберкойнов",
              "                g_ChangeCybercoins(-tflt1, 1);",
              "            }",
              "        }",
              "",
              "        //Проставляем маркер для вызова диалога с Келлером из PlayerActCode()",
              "        KellerExchange['DialogCallShip'] = Keller();",
              "        if(CurrentForm() == 'Ship') ButtonClick('Ship', 'Exit');",
              "    }",
              "    //Если игрок активирует \"Диггер\" впервые",
              "    else //if(!(DiggerRevelationStatus & 8))",
              "    {",
              "        dword comm_module = BuyDomikExtremal(NearCivilPlanet(Player()), 5, 1);",
              "        TransferShip(comm_module, StarPlanets(GalaxyStar(People), 0)); //На Меркурий, от греха подальше",
              "        ShipSetCoords(comm_module, CoordX(Player()), CoordY(Player()));",
              "        ShipType(comm_module, 'DomSecretary');",
              "        SetName(comm_module, CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellersSecretaryName'));",
              "        ShipDestroy(comm_module, 1); //Больше он нам будет не нужен",
              "",
              "        //Проставляем маркер для вызова диалога с \"секретарём\" Келлера из PlayerActCode()",
              "        KellerExchange['DialogCallShip'] = comm_module;",
              "        if(CurrentForm() == 'Ship') ButtonClick('Ship', 'Exit');",
              "    }",
              "}",
