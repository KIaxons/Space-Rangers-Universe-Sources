              "function CybercoinsCurExchangeRateMod(int direction = 0)",
              "{",
              "    ReCalcSystemsDomSerie();",
              "    if(!direction) result = PortionInDiapason(ControlledSystemsDomSerie[1], 0, GalaxyStars() - 2, 2.3, 0.3);",
              "    else result = PortionInDiapason(ControlledSystemsDomSerie[1], 0, GalaxyStars() - 2, 0.3, 2.3);",
              "}",
              "",
              "//Проверяем трюм ship и записываем все подходящие для сдачи Келлеру материалы в KellerExchange['MaterialsToSell']",
              "function FillKellerSellArray(dword ship)",
              "{",
              "    ArrayClear(KellerExchange['MaterialsToSell']);",
              "    ArrayDelete(KellerExchange['MaterialsToSell'], 0);",
              "",
              "    unknown tarr = newarray(1);",
              "    float coef = CybercoinsCurExchangeRateMod(1);",
              "    int found_cnt = 0;",
              "",
              "    int cnt = ShipItems(ship);",
              "    for(int i = 0; i < cnt; i = i + 1)",
              "    {",
              "        dword item = ShipItems(ship, i);",
              "        if(ItemInScript(item) == 1) continue; //Пропускаем все важные скриптовые",
              "        if(NoDropItem(item)) continue;",
              "",
              "        float value = 0.0;",
              "        int item_type = ItemType(item);",
              "        if(item_type <= t_CustomWeapon) //Оборудование или оружие",
              "        {",
              "            if(EqCustomFaction(item) == 'Klissan')",
              "            {",
              "                if(item_type <= t_DefGenerator) value = value + 5.0;",
              "                else value = value + 10.0;",
              "            }",
              "            else if(ItemOwner(item) == None)",
              "            {",
              "                if(item_type <= t_DefGenerator) value = value + 20.0;",
              "                else value = value + 25.0;",
              "            }",
              "",
              "            //Если установлен акрин, то проверяем его на интересность для Келлера",
              "            str path = BonusNumInCfg(EqSpecial(item));",
              "            if(path != '')",
              "            {",
              "                path = 'MicroModuls.' + path + '.';",
              "                if((int(CT(path + 'KellerExchange')) & 2)) value = value + RoundTo(coef * int(CT(path + 'KellerValue')), 0.1, 2);",
              "            }",
              "",
              "            //Если установлен ММ, то также проверяем его на интересность для Келлера",
              "            path = BonusNumInCfg(EqModule(item));",
              "            if(path != '')",
              "            {",
              "                //Установленные в оборудование микромодули представляют для Келлера меньший интерес, чем в отдельном виде",
              "                path = 'MicroModuls.' + path + '.';",
              "                if((int(CT(path + 'KellerExchange')) & 2)) value = value + (0.5 * RoundTo(coef * int(CT(path + 'KellerValue')), 0.1, 2));",
              "            }",
              "        }",
              "        else if(item_type == t_UselessItem)",
              "        {",
              "            path = 'UselessItems.' + EquipmentImageName(item) + '.';",
              "            if((int(CT(path + 'KellerExchange')) & 2)) value = value + RoundTo(coef * int(CT(path + 'KellerValue')), 0.1, 2);",
              "        }",
              "        else if(item_type == t_UselessCountableItem)",
              "        {",
              "            path = 'Items.CustomCountables.' + EquipmentImageName(item) + '.';",
              "            if((int(CT(path + 'KellerExchange')) & 2)) value = value + (RoundTo(coef * int(CT(path + 'KellerValue')), 0.1, 2) * ItemSize(item));",
              "        }",
              "        else if(item_type == t_MicroModule) //Отдельный микромодуль",
              "        {",
              "            path = 'MicroModuls.' + BonusNumInCfg(EqModule(item)) + '.';",
              "            if((int(CT(path + 'KellerExchange')) & 2)) value = value + RoundTo(coef * int(CT(path + 'KellerValue')), 0.1, 2);",
              "        }",
              "",
              "        if(!value) continue; //Данный предмет не представляет интереса для Келлера",
              "        value = 0.01 * Rnd(90, 110, Id(item) ^ GetObjectGenerationSeed(Keller())) * value; //Небольшой рандомизатор для разнообразия",
              "",
              "        ArrayDelete(tarr, 0);",
              "        ArrayAdd(tarr, Id(item), 'Id');",
              "        ArrayAdd(tarr, value, 'Value');",
              "        ArrayAdd(tarr, 0, 'WasSelected');",
              "        ArrayAdd(KellerExchange['MaterialsToSell'], tarr);",
              "        tarr = newarray(1);",
              "",
              "        found_cnt = found_cnt + 1;",
              "    }",
              "",
              "    cnt = ShipArts(ship);",
              "    for(i = 0; i < cnt; i = i + 1)",
              "    {",
              "        item = ShipArts(ship, i);",
              "        if(ItemInScript(item) == 1) continue; //Пропускаем все важные скриптовые",
              "        if(NoDropItem(item)) continue;",
              "",
              "        if(ItemType(item) >= t_ArtefactHull) path = 'Artefacts.' + EquipmentImageName(item) + '.';",
              "        else path = 'Artefacts.CustomArtefacts.' + EquipmentImageName(item) + '.'; //Кастомные арты проверяем по другому пути",
              "        if(!(int(CT(path + 'KellerExchange')) & 2)) continue; //Данный артефакт не представляет интереса для Келлера",
              "",
              "        value = max(1.0, value);",
              "        value = RoundTo(coef * CT(path + 'KellerValue'), 0.1, 2);",
              "        value = 0.01 * Rnd(90, 110, Id(item) ^ GetObjectGenerationSeed(Keller())) * value; //Небольшой рандомизатор для разнообразия",
              "",
              "        ArrayDelete(tarr, 0);",
              "        ArrayAdd(tarr, Id(item), 'Id');",
              "        ArrayAdd(tarr, value, 'Value');",
              "        ArrayAdd(tarr, 0, 'WasSelected');",
              "        ArrayAdd(KellerExchange['MaterialsToSell'], tarr);",
              "        tarr = newarray(1);",
              "",
              "        found_cnt = found_cnt + 1;",
              "    }",
              "",
              "    if(!ArrayDim(KellerExchange['MaterialsToSell'])) ArrayClear(KellerExchange['MaterialsToSell']);",
              "    result = found_cnt;",
              "}",
              "",
              "//Составляем новый array того, что Келлер готов предоставить игроку в обмен на киберкойны",
              "//Данную функцию не предполагается запускать слишком часто",
              "function FillKellerBuyArray()",
              "{",
              "    //Сперва удаляем все ранее созданные предметы из прошлого списка, если в массиве ещё что-то осталось",
              "    if(GetVariableType(KellerExchange['MaterialsToBuy', 0]))",
              "    {",
              "        for(int i = ArrayDim(KellerExchange['MaterialsToBuy']) - 1; i >= 0; i = i - 1)",
              "        {",
              "            dword item = GetItemFromVault(KellerExchange['MaterialsToBuy', i, 'VaultId']);",
              "            FreeItem(item);",
              "        }",
              "    }",
              "    ArrayClear(KellerExchange['MaterialsToBuy']);",
              "    ArrayDelete(KellerExchange['MaterialsToBuy'], 0);",
              "",
              "    unknown tarr = newarray(1);",
              "    float coef = CybercoinsCurExchangeRateMod();",
              "    int items_cnt = 0;",
              "",
              "    //Порядок и состав списка вещей на продажу:",
              "    //  1) Клиги (если выпадут) - пока не реализовано",
              "    //  2) Доминаторское оборудование и оружие (простое и акриновое)",
              "    //  3) Артефакты (разумеется, специальные келлерские, а не из ванилы)",
              "    //  4) Предметы типа Useless (сперва аналоги артов, а после них ошмётки)",
              "    //  5) Микромодули",
              "    //  6) Ноды (и прочие сыпучие, если когда-нибудь будут)",
              "",
              "    //Добавляем в список оборудование и оружие",
              "    for(int j = RndObject(9, 15, Keller()); j >= 1; j = j - 1)",
              "    {",
              "        if(ShuDomiksPlusInit) int cnt = t_CustomWeapon;",
              "        else cnt = t_Weapon15;",
              "",
              "        int item_type = t_Weapon16;",
              "        str item_str_type = '';",
              "        float value = 0;",
              "        int size = 0;",
              "        str act_code_path = '';",
              "        while(1)",
              "        {",
              "            item_type = RndObject(t_FuelTanks, cnt, Keller());",
              "            if(item_type != t_CustomWeapon)",
              "            {",
              "                if(item_type <= t_DefGenerator)",
              "                {",
              "                    item_str_type = EquipmentImageName(item_type);",
              "                    if(!int(CT('Items.' + item_str_type + '.KellerExchange')))",
              "                    {",
              "                        item_str_type = '';",
              "                        continue;",
              "                    }",
              "",
              "                    value = CT('Items.' + item_str_type + '.KellerValue');",
              "                    //Запоминаем минимально возможный (по ванильной механике) размер для данного предмета",
              "                    size = round(0.5 * CT('Items.' + item_str_type + '.AverageSize'));",
              "                    break;",
              "                }",
              "                else",
              "                {",
              "                    item_str_type = int(item_type);",
              "                    if(!int(CT('Items.Weapon.Stats.' + item_str_type + '.KellerExchange')))",
              "                    {",
              "                        item_str_type = '';",
              "                        continue;",
              "                    }",
              "",
              "                    value = CT('Items.Weapon.Stats.' + item_str_type + '.KellerValue');",
              "                    //Запоминаем минимально возможный (по ванильной механике) размер для данного предмета",
              "                    size = round(0.5 * CT('Items.Weapon.Stats.' + item_str_type + '.AverageSize'));",
              "                    break;",
              "                }",
              "            }",
              "            else",
              "            {",
              "                item_str_type = CT('Items.Weapon.CustomWeapon.ShuDomiksPlus.' + RndObject(1, int(CT('Items.Weapon.CustomWeapon.ShuDomiksPlus.Count')), Keller()));",
              "                if(!int(CT('Items.Weapon.CustomWeapon.' + item_str_type + '.KellerExchange')))",
              "                {",
              "                    item_str_type = '';",
              "                    continue;",
              "                }",
              "",
              "                value = CT('Items.Weapon.CustomWeapon.' + item_str_type + '.KellerValue');",
              "                //Запоминаем минимально возможный (по ванильной механике) размер для данного предмета",
              "                size = round(0.5 * CT('Items.Weapon.CustomWeapon.' + item_str_type + '.AverageSize'));",
              "                act_code_path = CT('Items.Weapon.CustomWeapon.' + item_str_type + '.WeaponActCode');",
              "                break;",
              "            }",
              "        }",
              "",
              "        int item_TL = min(GTL + RndObject(0, 1, Keller()), 8);",
              "        if(item_type != t_CustomWeapon) item = CreateEquipment(item_type, round(RndFloat(0.95, 1.75, Keller()) * size), item_TL, Kling);",
              "        else item = CreateCustomWeapon(item_str_type, round(RndFloat(0.95, 1.75, Keller()) * size), item_TL, Kling);",
              "",
              "        if(item_type != t_Scaner) ItemSubrace(item, 1);",
              "        //Доминаторские сканеры в продаже могут иметь любую серию",
              "        else ItemSubrace(item, RndObject(0, 2, Keller()));",
              "",
              "        //С шансом в 25% накладываем акрин (если найдётся подходящий)",
              "        if(!RndObject(0, 3, Keller()))",
              "        {",
              "            int bon = SpecialToEquipment(item, item_TL);",
              "            //Если в бонусе не прописана отдельная цена в cc., то выставляем цену по умолчанию",
              "            if(~bon)",
              "            {",
              "                //Сразу же разблокируем доминаторскую модификацию для игрока",
              "                if(~findsubstr(BonusNumInCfg(bon), 'DomModLocked'))",
              "                {",
              "                    bon = FindBonusByNameInCfg(Format(BonusNumInCfg(bon), 'Locked', 'Unlocked', ''));",
              "                    SpecialToEquipment(-1, item);",
              "                    SpecialToEquipment(bon, item);",
              "                }",
              "",
              "                //Если акрин не имеет собственного имени, добавляем в название предмета цветовые тэги",
              "                if(BonusName(bon) == '') SetName(item, '<color=255,167,84>' + Name(item) + '</color>');",
              "",
