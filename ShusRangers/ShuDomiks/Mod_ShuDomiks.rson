          "Code":          
          [
              "//Составляем список выбора предметов на продажу",
              "tstr2 = '</color>' + CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.PlayerSetItemSell');",
              "tstr3 = '</color>' + CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.PlayerRemoveItemSell');",
              "",
              "for(int i = 0; i < ArrayDim(KellerExchange['MaterialsToSell']); i = i + 1)",
              "{",
              "    dword item = IdToItem(KellerExchange['MaterialsToSell', i, 'Id']);",
              "    tstr4 = FloatToString(KellerExchange['MaterialsToSell', i, 'Value'], 2);",
              "    if(substr(tstr4, count(tstr4) - 1) == '0') tstr4 = substr(tstr4, 0, count(tstr4) - 1);",
              "",
              "    if(!KellerExchange['MaterialsToSell', i, 'WasSelected']) tstr1 = Format(tstr2, '<Item'+'>', Name(item), '<Cost'+'>', tstr4);",
              "    else tstr1 = Format(tstr3, '<Item'+'>', Name(item), '<Cost'+'>', tstr4);",
              "",
              "    //Корректируем цвет названия для акринов на белом фоне",
              "    tstr1 = Format(tstr1, '<color=255,167,84>', '<color=240,100,30>', '');",
              "",
              "    InjectAnswer('KellerSellSetDialog', 'snap~' + tstr1, i);",
              "}",
              "",
              "InjectAnswer('MainDiggerDialog', CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.PlayerBack'), 0);"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  0,
          "Pos.y":  20,
          "Parent":  390,
          "#":  387,
          "Total.Lines":  20,
          "Code.Type":  "Turn",
          "Code":          
          [
              "//Составляем список выбора предметов на покупку",
              "tstr2 = '</color>' + CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.PlayerSetItemBuy');",
              "tstr3 = '</color>' + CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.PlayerRemoveItemBuy');",
              "",
              "for(int i = 0; i < ArrayDim(KellerExchange['MaterialsToBuy']); i = i + 1)",
              "{",
              "    dword item = GetItemFromVault(KellerExchange['MaterialsToBuy', i, 'VaultId'], 0);",
              "    tstr4 = FloatToString(KellerExchange['MaterialsToBuy', i, 'Value'], 2);",
              "    if(substr(tstr4, count(tstr4) - 1) == '0') tstr4 = substr(tstr4, 0, count(tstr4) - 1);",
              "",
              "    if(!KellerExchange['MaterialsToBuy', i, 'WasSelected']) tstr1 = Format(tstr2, '<Item'+'>', Name(item), '<Cost'+'>', tstr4);",
              "    else tstr1 = Format(tstr3, '<Item'+'>', Name(item), '<Cost'+'>', tstr4);",
              "",
              "    //Корректируем цвет названия для акринов на белом фоне",
              "    tstr1 = Format(tstr1, '<color=255,167,84>', '<color=240,100,30>', '');",
              "",
              "    InjectAnswer('KellerBuyStatsDialog', 'snap~' + tstr1, i);",
              "}",
              "",
              "InjectAnswer('MainDiggerDialog', CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.PlayerBack'), 0);"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  0,
          "Pos.y":  20,
          "Parent":  396,
          "#":  397,
          "Total.Lines":  133,
          "Code.Type":  "Turn",
          "Code":          
          [
              "dword item = GetItemFromVault(KellerExchange['MaterialsToBuy', GAnswerData, 'VaultId'], 0);",
              "",
              "if(!KellerExchange['MaterialsToBuy', GAnswerData, 'WasSelected'])",
              "{",
              "    int item_type = ItemType(item);",
              "    tstr1 = '';",
              "",
              "    //Заполнение карточки оборудования или оружия",
              "    if(item_type >= t_FuelTanks && item_type <= t_CustomWeapon)",
              "    {",
              "        //Выписываем название и размер предмета",
              "        tstr1 = tstr1 + '<color=255,240,100>- ' + Name(item) + ' [<color=0,132,15>' + ItemSize(item) + '</color>] -</color>' + Format('<br>');",
              "",
              "        //Выписываем ТУ предмета (для оружия здесь же указывается его тип)",
              "        if(item_type >= t_Weapon1)",
              "        {",
              "            int tech_level = ItemLevel(item);",
              "            if(tech_level >= 8) tstr2 = 'H';",
              "            else if(tech_level == 7) tstr2 = 'G';",
              "            else if(tech_level == 6) tstr2 = 'F';",
              "            else if(tech_level == 5) tstr2 = 'E';",
              "            else if(tech_level == 4) tstr2 = 'D';",
              "            else if(tech_level == 3) tstr2 = 'C';",
              "            else if(tech_level == 2) tstr2 = 'B';",
              "            else if(tech_level <= 1) tstr2 = 'A';",
              "",
              "            if(GetEquipmentStats(item, 3) == 0) tstr1 = tstr1 + CT('FormInfo.ISType') + ' <color=255,240,100>' + CT('FormInfo.Equipment.EnergyType') + '</color>' + ' <color=255,240,100>(' + tstr2 + ')</color>' + Format('<br>');",
              "            else if(GetEquipmentStats(item, 3) == 1) tstr1 = tstr1 + CT('FormInfo.ISType') + ' <color=255,240,100>' + CT('FormInfo.Equipment.SplinterType') + '</color>' + ' <color=255,240,100>(' + tstr2 + ')</color>' + Format('<br>');",
              "            else tstr1 = tstr1 + CT('FormInfo.ISType') + ' <color=255,240,100>' + CT('FormInfo.Equipment.MissileType') + '</color>' + ' <color=255,240,100>(' + tstr2 + ')</color>' + Format('<br>');",
              "        }",
              "        else tstr1 = tstr1 + DeleteTags(CT('FormInfo.Equipment.Tech')) + '<color=255,240,100>' + ItemLevel(item) + '</color>' + Format('<br>');",
              "",
              "        //Выписываем статы предмета",
              "        if(item_type >= t_Weapon1)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Damage')), ' -', GetEquipmentStats(item, ' <MinDamage'+'></color>-<MaxDamage'+'><Bonus'+'>')) + ', ' + Format(DeleteTags(CT('FormInfo.Equipment.Radius')), ' ', GetEquipmentStats(item, ' <Radius'+'>'));",
              "            //Для ракетного оружия также добавляем максимальный боезапас",
              "            if(GetEquipmentStats(item, 3) == 2) tstr1 = tstr1 + ', ' + Format(CT('FormInfo.Equipment.MaxAmmo'), '<MaxCount'+'>', GetEquipmentStats(item, '<MaxCount'+'>')) + Format('<br>');",
              "            else tstr1 = tstr1 + Format('<br>');",
              "        }",
              "        else if(item_type == t_Engine)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Speed')), ' ', GetEquipmentStats(item, ' <Speed'+'>')) + ', ' + Format(DeleteTags(CT('FormInfo.Equipment.Jump')), ' ', GetEquipmentStats(item, ' <Parsec'+'>')) + Format('<br>');",
              "        }",
              "        else if(item_type == t_FuelTanks)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Fuel')), ' ', GetEquipmentStats(item, ' <Capacity'+'>')) + Format('<br>');",
              "        }",
              "        else if(item_type == t_Radar)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Radar')), ' ', GetEquipmentStats(item, ' <Radius'+'>')) + Format('<br>');",
              "        }",
              "        else if(item_type == t_Scaner)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Scaner')), ' ', GetEquipmentStats(item, ' <Percent'+'>')) + Format('<br>');",
              "        }",
              "        else if(item_type == t_RepairRobot)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Droid')), ' ', GetEquipmentStats(item, ' <RecoverHitPoints'+'>')) + Format('<br>');",
              "        }",
              "        else if(item_type == t_CargoHook)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Hook')), ' ', GetEquipmentStats(item, ' <PickUpSize' + '>')) + ', ' + Format(DeleteTags(CT('FormInfo.Equipment.HookRadius')), ' ', GetEquipmentStats(item, ' <Radius'+'>')) + ', ' + Format(DeleteTags(CT('FormInfo.Equipment.HookSpeed')), ' -', GetEquipmentStats(item, ' <SpeedMin'+'></color>-<SpeedMax'+'>')) + Format('<br>');",
              "        }",
              "        else if(item_type == t_DefGenerator)",
              "        {",
              "            tstr1 = tstr1 + Format(DeleteTags(CT('FormInfo.Equipment.Defend')), ' ', GetEquipmentStats(item, ' <Percent' + '>')) + Format('<br>');",
              "        }",
              "",
              "        //Выписываем надёжность предмета",
              "        float flt1 = ItemFragility(item);",
              "        float flt2 = 0.60;",
              "        float flt3 = 5.00;",
              "        int grade = 0;",
              "        for(i = 0; i <= 6; i = i + 1)",
              "        {",
              "            if(i <= 3) flt2 = flt2 + 0.10;",
              "            else flt2 = flt2 + 0.20;",
              "            if(abs(flt1 - flt2) < flt3)",
              "            {",
              "                flt3 = abs(flt1 - flt2);",
              "                grade = i;",
              "            }",
              "        }",
              "        tstr1 = tstr1 + CT('FormInfo.Equipment.Reliability') + '<color=255,240,100>' + CT('FormInfo.Equipment.QualityGrade' + grade) + '</color>' + CT(\"Script.Mod_ExpAutoSearch.0\");",
              "",
              "        //Добавляем описание акрина, если он есть",
              "        int bon = EqSpecial(item);",
              "        if(~bon)",
              "        {",
              "            tstr1 = tstr1 + Format('<br>') + ' ' + Format('<br>');",
              "            //if(item_type >= t_Weapon1) tstr1 = tstr1 + CT('FormInfo.Equipment.DomWeaponAcryn');",
              "            //else tstr1 = tstr1 + CT('FormInfo.Equipment.DomAcryn');",
              "            tstr1 = tstr1 + Format('<br>') + BonusText(bon);",
              "        }",
              "",
              "        tstr1 = Format(tstr1,",
              "        '<color=0,255,0>', '<color=0,132,15>', //Цифры бонуса от ММ",
              "        '<color=248,252,248>', '', //Белый цвет текста описания акрина",
              "        '<color=255,167,84>', '<color=240,100,30>', //Оранжевый цвет цифр бонуса от акрина",
              "        '');",
              "    }",
              "    else if(item_type == t_MicroModule) tstr1 = '<color=0,50,200>- ' + Name(item) + ' -</color>' + Format('<br>') + BonusText(EqModule(item));",
              "    else if(item_type <= t_Artefact2) //Кастомный артефакт",
              "    {",
              "        tstr1 = '<color=0,50,200>- ' + Name(item)+ ' [<color=0,132,15>' + ItemSize(item) + '</color>] -</color>' + Format('<br>') + CT('Artefacts.CustomArtefacts.' + EquipmentImageName(item) + '.Text');",
              "        tstr1 = Format(tstr1,",
              "                       '<Data1'+'>', CustomArtData(item, 1),",
              "                       '<Data2'+'>', CustomArtData(item, 2),",
              "                       '<Data3'+'>', CustomArtData(item, 3),",
              "                       '<TextData1'+'>', CustomArtTextData(item, 1),",
              "                       '<TextData2'+'>', CustomArtTextData(item, 2),",
              "                       '<TextData3'+'>', CustomArtTextData(item, 3));",
              "    }",
              "    else if(item_type <= t_Hull) //Ванильный артефакт",
              "    {",
              "        tstr1 = '<color=0,50,200>- ' + Name(item)+ ' [<color=0,132,15>' + ItemSize(item) + '</color>] -</color>' + Format('<br>') + CT('Artefacts.' + EquipmentImageName(item) + '.Text');",
              "    }",
              "    else if(item_type == t_UselessItem)",
              "    {",
              "        tstr1 = UselessItemText(item);",
              "        if(tstr1 == '') tstr1 = '<color=0,50,200>- ' + Name(item) + ' [<color=0,132,15>' + ItemSize(item) + '</color>] -</color>' + Format('<br>') + CT('UselessItems.' + EquipmentImageName(item) + '.Text');",
              "        else tstr1 = '<color=0,50,200>- ' + Name(item)+ ' [<color=0,132,15>' + ItemSize(item) + '</color>] -</color>' + Format('<br>') + tstr1;",
              "    }",
              "    else if(item_type == t_Protoplasm) tstr1 = '<color=0,50,200>- ' + Name(item) + ' -</color>' + Format('<br>') + CT('Items.Nod.Text'); //Обыкновенные ноды",
              "    else if(item_type == t_UselessCountableItem) tstr1 = '<color=0,50,200> - ' + Name(item) + ' -</color>' + Format('<br>') + CT('Items.CustomCountables.' + EquipmentImageName(item) + '.Text');",
              "    else throw 'Invalid item type in Keller MaterialsToBuy array!';",
              "",
              "    tstr1 = tstr1 + Format('<br>') + ' ' + Format('<br>') + CT('Script.Mod_ShuDomiks.MainDiggerDialog.KellerExchange.MessageAddItemBuy');",
              "    if(MessageBoxYesNo(tstr1)) KellerExchange['MaterialsToBuy', GAnswerData, 'WasSelected'] = 1;",
              "}",
              "//Если игрок захотел убрать предмет из списка выбранных для сделки",
              "else KellerExchange['MaterialsToBuy', GAnswerData, 'WasSelected'] = 0;"          
          ]        
        },
