              "                    if(temp_str_1=='Ranger' || temp_str_1=='RangerPirate' || temp_str_1=='Freelancer' || temp_str_1=='Merc') sh=25;",
              "                    else if(temp_str_1=='Smuggler') sh=12;",
              "                    else if(temp_str_1=='Diplomat') sh=12;",
              "                    else if(temp_str_1=='Transport' || temp_str_1=='Liner') sh=5;",
              "                    else if(temp_str_1=='Pirate' && PirateType(s)) sh=12;",
              "                    else if(temp_str_1=='Pirate') sh=16;",
              "                    else if(temp_str_1=='Warrior' && ShipSubType(s)) sh=85;",
