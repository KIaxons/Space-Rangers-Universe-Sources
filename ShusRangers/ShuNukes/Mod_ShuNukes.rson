              "        star_opora=ShipStar(Player());",
              "        for(star_num=0; star_num<GalaxyStars(); star_num=star_num+1)",
              "        {",
              "            starka=StarNearbyStars(star_opora,star_num);",
              "            shipcantz=StarShips(starka);",
              "            for(ship_checked=0; ship_checked<shipcantz; ship_checked=ship_checked+1)",
