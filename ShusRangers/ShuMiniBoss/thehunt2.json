"}",
"else",
"{",
"guns=guns+1; AddItemToShip(ship,give_eq_item_var);",
"}",
"}",
"else if (1==0)",
"{",
"guns=guns+1; AddItemToShip(ship,give_eq_item_var);",
"}",
"else FreeItem(give_eq_item_var);",
"}",
"else AddItemToShip(ship,give_eq_item_var);",
