"if (race==PirateClan && !Rnd(0,3)) eq_type=t_Weapon17;",
"if (eq_type==t_Weapon6) i=Rnd(2,3);",
"if (eq_type==t_Weapon4) i=Rnd(3,4);",
"if (eq_type==t_Weapon3 || eq_type==t_Weapon17) i=4;",
