              "                //Повышается размер корпуса, устраняем возможный перевес",
              "                ItemSize(a, ItemSize(a) + RndObject(289, 307, s));",
              "                ShipCalcParam(s);",
              "                if(ShipFreeSpace(s) < 0) ItemSize(a, ItemSize(a) + abs(ShipFreeSpace(s)) + RndObject(1, 5, s));",
              "                HullHP(a, ItemSize(a));",
              "                //Автосортировка, улучшение и ремонт всех новодобавленных вещей",
              "                ArrangeItems(s);",
              "                ShipImproveItems(s, 10);",
              "                ShipRepairEq(s);",
              "                //Обновляем скин на недавно установленный акрин",
              "                Chameleon(s);",
              "            }",
              "",
              "            //Спавняться миньоны охотника",
              "            for(i=1; i<=5; i=i+1)",
              "            {",
              "                PlanetRace(PlanetPirateClan(), RndObject(Maloc, Gaal, PlanetPirateClan()));",
              "                es = BuyPirate(PlanetPirateClan());",
              "                NoTargetToShip(es, 2);",
              "                ShipJoin(gEscort, es);",
              "                //Добавляется мусор",
              "                //Ящик с золотом",
              "                ShipAddDropItem(es,CreateQuestItem('Usl_PTQ70',RndObject(0, 4, es)));",
              "                //Кейс с документами",
              "                ShipAddDropItem(es,CreateQuestItem('Usl_RandomItem_04',RndObject(0, 4, es)));",
              "",
              "                //Добавляются артефакты",
              "                a=CreateArt(t_ArtefactHull,6);",
              "                AddItemToShip(es,a);",
              "                if(RndObject(1, 100, es) <= 95) NoDropItem(a, 1);",
              "                a=CreateArt(t_ArtefactDroid,6);",
              "                AddItemToShip(es,a);",
              "                if(RndObject(1, 100, es) <= 95) NoDropItem(a, 1);",
              "                a=CreateArt(t_ArtefactNano,6);",
              "                AddItemToShip(es,a);",
              "                if(RndObject(1, 100, es) <= 95) NoDropItem(a, 1);",
              "                a=CreateArt(t_ArtDecelerate,6);",
              "                AddItemToShip(es,a);",
              "                if(RndObject(1, 100, es) <= 95) NoDropItem(a, 1);",
              "",
              "                //Добавляется оборудование",
              "                j=ShipPilotRace(es);",
              "                //Пигамарный двигатель I",
              "                a=CreateEquipment(t_Engine,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('395'), a);",
              "                AddItemToShip(es,a);",
              "                //Пигамарный топливный бак I",
              "                a=CreateEquipment(t_FuelTanks,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('394'), a);",
              "                AddItemToShip(es,a);",
              "                //Пигамарный дроид I",
              "                a=CreateEquipment(t_RepairRobot,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('398'), a);",
              "                AddItemToShip(es,a);",
              "                //Пигамарный захват I",
              "                a=CreateEquipment(t_CargoHook,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('399'), a);",
              "                AddItemToShip(es,a);",
              "                //Пигамарный генератор защиты I",
              "                a=CreateEquipment(t_DefGenerator,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('400'), a);",
              "                AddItemToShip(es,a);",
              "                //Пигамарный сканер I",
              "                a=CreateEquipment(t_Scaner,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('397'), a);",
              "                AddItemToShip(es,a);",
              "                //Пигамарный радар I",
              "                a=CreateEquipment(t_Radar,RndObject(35, 70, es),GalaxyTechLevel(),j);",
              "                SpecialToEquipment(FindBonusByNameInCfg('396'), a);",
              "                AddItemToShip(es,a);",
              "",
              "                //Добавляется оружие",
              "                a=CreateEquipment(t_Weapon4,RndObject(28, 45, es),GalaxyTechLevel(),j);",
              "                AddItemToShip(es,a);",
              "                a=CreateEquipment(t_Weapon4,RndObject(28, 45, es),GalaxyTechLevel(),j);",
              "                AddItemToShip(es,a);",
              "                a=CreateEquipment(t_Weapon4,RndObject(28, 45, es),GalaxyTechLevel(),j);",
              "                AddItemToShip(es,a);",
              "                a=CreateEquipment(t_Weapon4,RndObject(28, 45, es),GalaxyTechLevel(),j);",
              "                AddItemToShip(es,a);",
              "                a=CreateEquipment(t_Weapon4,RndObject(28, 45, es),GalaxyTechLevel(),j);",
              "                AddItemToShip(es,a);",
              "                a=ShipItems(es,0);",
              "",
              "                //Выдаём корпуса",
              "                j = ShipPilotRace(es);",
              "                if(j == Maloc) SpecialToEquipment(FindBonusByNameInCfg('ShuMM_RattletropHull3'), a);",
              "                else if(j == Peleng) SpecialToEquipment(FindBonusByNameInCfg('ShuMM_ShmyakushHull3'), a);",
              "                else if(j == People) SpecialToEquipment(FindBonusByNameInCfg('ShuMM_ProudwingHull3'), a);",
              "                else if(j == Fei) SpecialToEquipment(FindBonusByNameInCfg('ShuMM_FuravanHull3'), a);",
              "                else if(j == Gaal) SpecialToEquipment(FindBonusByNameInCfg('ShuMM_PickuupHull3'), a);",
              "",
              "                //Устраняем возможный перевес",
              "                ShipCalcParam(es);",
              "                if(ShipFreeSpace(es) < 0) ItemSize(a,ItemSize(a)+abs(ShipFreeSpace(es))+RndObject(1, 5, es));",
              "                HullHP(a, ItemSize(a));",
              "                //Автосортировка, улучшение и ремонт всех новодобавленных вещей",
              "                ArrangeItems(es);",
