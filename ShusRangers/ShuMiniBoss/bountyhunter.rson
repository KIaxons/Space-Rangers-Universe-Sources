          "Code":          
          [
              "//Проверяем, не пора ли уже выпускать охотника",
              "if(GBountyHunterStartDate <= CurTurn())",
              "{",
              "    //Если охотник ещё не выпускался ранее",
              "    if(!onceatatime)",
              "    {",
              "        //Подбирается список подходящих планет для спавна",
              "        ArrayClear(spawn_planets);",
              "        for(j = 0; j < GalaxyStars(); j = j + 1)",
              "        {",
              "            cur_star = GalaxyStar(j);",
              "            //Только системы Коалиции",
              "            if(StarOwner(cur_star) != 0) continue;",
              "",
              "            for(i = 0; i < StarPlanets(cur_star); i = i + 1)",
              "            {",
              "                cur_pl = StarPlanets(cur_star, i);",
              "                //Только населённые планеты Коалиции",
              "                if(PlanetOwner(cur_pl) <= 4) ArrayAdd(spawn_planets, cur_pl);",
              "            }",
              "        }",
              "",
              "        //Если нашли хотя бы одну подходящую планету",
              "        if(ArrayDim(spawn_planets) > 1)",
              "        {",
              "            //Из составленного списка рандомно выбираем планету",
              "            cur_pl = spawn_planets[RndObject(1, ArrayDim(spawn_planets) - 1, cur_star)];",
              "            //Спавнится охотник",
              "            for(i = 1; i <= 1; i = i + 1)",
              "            {",
              "                s = BuyRanger(cur_pl);",
              "                ShipJoin(HunterGroup, s);",
              "                NoTargetToShip(s, 2);",
              "                //Добавляется мусор",
              "                //Ящик с золотом",
              "                if(RndObject(1, 100, s) <= 65) ShipAddDropItem(s, CreateQuestItem('Usl_PTQ70', RndObject(0, 4, s)));",
