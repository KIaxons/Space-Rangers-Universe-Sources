              "                    else if(check == -1) tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskAgain');",
              "                    else tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskTwice');",
              "                    AddDialogInject('Mod_ShuMercsHQ:OwnBaseDialogMB', '', tstr1, 10);",
              "                }",
              "                //Этот ответ игрока запускает диалог с непосредственным строительством собственного штаба наёмников",
              "                else if(check >= 3 && check <= 5)",
              "                {",
              "                    if(check == 3) tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskBuild');",
              "                    else if(check == 4) tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskBuildAgain');",
              "                    else tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskBuildTwice');",
              "                    AddDialogInject('Mod_ShuMercsHQ:OwnBaseDialogMB', '', tstr1, 10);",
              "                }",
              "                //Диалог о переименовании организации наёмников игрока",
              "                else if(check == 6)",
              "                {",
              "                    tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.PlayerAskRename');",
              "                    AddDialogInject('Mod_ShuMercsHQ:OwnBaseRenameDialogMB', '', tstr1, 10);",
              "                }",
              "            }",
              "            else //Игрок начал, но ещё не завершил квест с Ципцилотом",
              "            {",
              "                if(!check) tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAsk');",
              "                else if(check == -1) tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskAgain');",
              "                else tstr1 = '</color>' + CT('Script.Mod_ShuMercs.CreatingPlayerOrgDialogs.MBPlayerAskTwice');",
              "                AddDialogBlock(tstr1, 1);",
              "                AddDialogInject('Mod_ShuMercsHQ:OwnBaseDialogMB', '', tstr1, 10);",
              "            }",
              "        }",
              "",
              "        //Поправить отношения с наёмниками",
              "        if(ShipType(cur_ruin) == 'MB' && !MercOrganisationsData['NegotiationsInProgress'])",
              "        {",
              "            tstr1 = CT('Script.Mod_ShuMercs.' + dialogs_path + '.PlayerAskFixRelations');",
              "            AddDialogInject('MB_FixRelations', '', tstr1, 5);",
              "        }",
              "    }",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -20,
          "Pos.y":  0,
          "Parent":  407,
          "#":  408,
          "Total.Lines":  18,
          "Code.Type":  "Turn",
          "Code":          
          [
              "//Данный стейт используется и заполняется в том числе из скрипта ShuMercsHQ",
              "//Смысл данного стейта в том, чтобы просто поскорее вывести любого наёмника из игры незаметно для игрока",
              "if(!ShipInNormalSpace(CurShip))",
              "{",
              "    ShipDeleteCustomShipInfo(CurShip, 'ShuMercsHQ_StrikeGroupMercInfo'); //Нужны для контроля реальной смерти бывших наёмников из ударной группы",
              "    OrderNone(CurShip);",
              "    ShipDestroy(CurShip, 1);",
              "    exit;",
              "}",
              "",
              "//Посадка или прыжок",
              "if(ShipOrder(CurShip) == 2 || ShipOrder(CurShip) == 3) exit;",
              "",
              "//Отправляем бывшего наёмника (или какой-то другой корабль?) умирать где-то вне поля зрения игрока",
              "OrderLock(CurShip, 1);",
              "dword cur_star = ShipStar(CurShip);",
              "if(StarOwner(cur_star) != 1) OrderLanding(CurShip, NearCivilPlanet(CurShip));",
              "else OrderJump(CurShip, StarNearbyStars(cur_star, RndObject(1, 5, CurShip)));"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -20,
          "Pos.y":  20,
          "Parent":  175,
          "#":  409,
          "Total.Lines":  87,
          "Code.Type":  "Turn",
          "Code":          
          [
              "int org_num = GetData(3);",
              "",
              "//Вручную корректируем характер наёмника",
              "//Фрилансер",
              "if(!org_num)",
              "{",
              "    ShipStatus(CurShip, 0, 15); //Торговец",
              "    ShipStatus(CurShip, 1, 42); //Пират",
              "    ShipStatus(CurShip, 2, 43); //Воин",
              "}",
              "//Топор",
              "else if(org_num == 1)",
              "{",
              "    if(!GroupCount(RedLeader))",
              "    {",
              "        MakeFreelancerFromMerc(CurShip);",
              "        exit;",
              "    }",
              "",
              "    ShipStatus(CurShip, 0, 0); //Торговец",
              "    ShipStatus(CurShip, 1, 75); //Пират",
              "    ShipStatus(CurShip, 2, 25); //Воин",
              "}",
              "//Атлант",
              "else if(org_num == 2)",
              "{",
              "    if(!GroupCount(AtlantLeader))",
              "    {",
              "        MakeFreelancerFromMerc(CurShip);",
              "        exit;",
              "    }",
              "",
              "    ShipStatus(CurShip, 0, 0); //Торговец",
              "    ShipStatus(CurShip, 1, 40); //Пират",
              "    ShipStatus(CurShip, 2, 60); //Воин",
              "}",
              "//Фэй",
              "else //if(org_num == 3)",
              "{",
              "    if(!GroupCount(FeiLeader))",
              "    {",
              "        MakeFreelancerFromMerc(CurShip);",
              "        exit;",
              "    }",
              "",
              "    ShipStatus(CurShip, 0, 0); //Торговец",
              "    ShipStatus(CurShip, 1, 15); //Пират",
              "    ShipStatus(CurShip, 2, 85); //Воин",
              "}",
              "",
              "if(ShipType(CurShip) != 'Freelancer')",
              "{",
              "    //Если игрок (или его миньён) атаковал наёмника или босса конкурирующей организации, то это сразу влечёт за собой конфликт и пересчёт отношения организаций",
              "    if(Hit(CurShip, 0))",
              "    {",
              "        //Топор",
              "        if(org_num == 1)",
              "        {",
              "            if(MercOrganisationsData['NegotiationsInProgress'] && MercOrganisationsData['NegotiationsTargetSide'] == org_num) red_rel = rem;",
              "",
              "            red_rel = red_rel - 5;",
              "            atlant_rel = atlant_rel + RndObject(1, 2, CurShip);",
              "            fei_rel = fei_rel + 3;",
              "        }",
              "        //Атлант",
              "        else if(org_num == 2)",
              "        {",
              "            if(MercOrganisationsData['NegotiationsInProgress'] && MercOrganisationsData['NegotiationsTargetSide'] == org_num) atlant_rel = rem;",
              "",
              "            red_rel = red_rel + RndObject(1, 2, CurShip);",
              "            atlant_rel = atlant_rel - 4;",
              "            fei_rel = fei_rel + RndObject(1, 2, CurShip);",
              "        }",
              "        //Фэй",
              "        else //if(org_num == 3)",
              "        {",
              "            if(MercOrganisationsData['NegotiationsInProgress'] && MercOrganisationsData['NegotiationsTargetSide'] == org_num) fei_rel = rem;",
              "",
              "            red_rel = red_rel + 3;",
              "            atlant_rel = atlant_rel + RndObject(1, 2, CurShip);",
              "            fei_rel = fei_rel - 6;",
              "        }",
              "",
              "        //И сразу сбрасываем маркер, чтобы не мешал",
              "        Hit(CurShip, 0, 0);",
              "    }",
              "}"          
          ]        
        },
