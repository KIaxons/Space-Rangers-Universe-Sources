          "Parent":  -1,
          "#":  25,
          "Total.Lines":  318,
          "Code.Type":  "Turn",
          "Code":          
          [
              "//Раздача юзлессов",
              "//Перебираем все новые корабли в галактике по заданным фильтрам",
              "if(GalaxyShipId() > lastShipId)",
              "{",
              "    tmp = BuildListOfNewShips(tArr, lastShipId, 2+4+8+16, 1+2+4+8+16+128, 1, '', 'Militiaman1,,Militiaman2,Militiaman3,Rebel,PirateRunner,RangerPirate,RangerExplorer,RangerPirateExplorer,Smuggler,EliteWarrior,Merc,Freelancer,RedLeader,AtlantLeader,FeiLeader,Khan,Headhunter');",
              "    //Если найден хотя бы один корабль, выдаем юзлессы",
              "    if(tmp)",
              "    {",
              "        for(i = 0; i < ArrayDim(tArr); i = i + 1)",
              "        {",
              "            //Обнуляем флаг для раздачи юзлессов",
              "            tmp=0;",
              "            //Выбираем корабль",
              "            tmp2=tArr[i];",
              "            //На всякий случай пропускаем корабли в гипере",
              "            if(ShipInHyperSpace(tmp2)) continue;",
              "            //Устанавливаем шанс на выдачу того или иного юзлесса",
              "            chance=0;",
              "            //Рейнджеры",
              "            tstr1 = ShipType(tmp2);",
              "            if(tstr1 == 'Ranger' || tstr1 == 'RangerPirate' || tstr1 == 'RangerExplorer' || tstr1 == 'RangerPirateExplorer') chance = 40;",
              "            //Мирные корабли",
              "            else if(tstr1 == 'Diplomat' || tstr1 == 'Transport' || tstr1 == 'Smuggler' || tstr1 == 'Liner') chance = 35;",
              "            //Пираты",
              "            else if(tstr1 == 'Pirate' || tstr1 == 'Rebel' || tstr1 == 'PirateRunner' || tstr1 == 'Khan') chance = 30;",
              "            //Военные",
              "            else if(tstr1 == 'Warrior' || tstr1 == 'EliteWarrior' || tstr1 == 'Militiaman1' || tstr1 == 'Militiaman2' || tstr1 == 'Militiaman3') chance = 25;",
              "",
              "            //Случайно определяем шанс выдачи",
              "            if(Rnd(1, 100) <= chance)",
              "            {",
              "                //Формируется массив из всех возможных юзлессов для данного корабля",
              "                ArrayClear(hit_list);",
              "                //Пираты",
              "                if(ShipType(tmp2) == 'Pirate' || ShipType(tmp2) == 'Rebel' || ShipType(tmp2) == 'PirateRunner' || ShipType(tmp2) == 'Khan')",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_11',PirateClan); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                //Малоки",
              "                if(!ShipPilotRace(tmp2)) {",
              "                    if(ShipType(tmp2)!='Warrior' && ShipType(tmp2)!='WarriorBig') {",
              "                        tmp3=CreateQuestItem('Usl_64',Maloc); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2) == 'Diplomat' || ShipType(tmp2) == 'Ranger' || ShipType(tmp2) == 'Warrior' || ShipType(tmp2) == 'EliteWarrior' || ShipType(tmp2) == 'Militiaman1' || ShipType(tmp2) == 'Militiaman2' || ShipType(tmp2) == 'Militiaman3' ) {",
              "                        tmp3=CreateQuestItem('Usl_2',Maloc); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                }",
              "                //Пеленги",
              "                if(ShipPilotRace(tmp2)==1) {",
              "                    if(ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Ranger' || ShipTypeN(tmp2)==t_Warrior)",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_12',Peleng); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2)!='Warrior' && ShipType(tmp2)!='WarriorBig')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_70',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Ranger' || ShipType(tmp2) == 'Liner')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_16',Peleng); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2)!='WarriorBig' && ShipType(tmp2)!='Warrior')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_19',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2) == 'Diplomat' || ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Ranger')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_22',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipTypeN(tmp2)==t_Pirate && !Rnd(0,2) || ShipType(tmp2) == 'Ranger' && Rnd(0,1) || ShipType(tmp2) == 'RangerPirate' && Rnd(0,2) || ShipType(tmp2) == 'Smuggler' && Rnd(0,3)) {",
              "                        tmp3=CreateQuestItem('Usl_25',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Ranger' || ShipType(tmp2) == 'RangerPirate')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_26',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if (ShipType(tmp2) == 'Ranger' && !Rnd(0,2) || ShipType(tmp2) == 'RangerPirate' && !Rnd(0,2) || ShipType(tmp2) == 'Diplomat')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_28',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if(ShipType(tmp2) == 'Diplomat' && ShipOwner(tmp2)>1 && ShipOwner(tmp2)<4 || ShipType(tmp2) == 'Ranger' && ShipOwner(tmp2)>1 && ShipOwner(tmp2)<4 || ShipType(tmp2) == 'Diplomat' && ShipOwner(tmp2)>1 && ShipOwner(tmp2)<4 || !Rnd(0,2) && ShipTypeN(tmp2)==t_Warrior && ShipOwner(tmp2)>1 && ShipOwner(tmp2)<4)",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_29',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    tmp3=CreateQuestItem('Usl_30',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                //Фэяне",
              "                if(ShipPilotRace(tmp2)==3)",
              "                {",
              "                    if(ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Ranger')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_33',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                    if (ShipType(tmp2) == 'RangerPirate' && !Rnd(0,2) || ShipType(tmp2) == 'Ranger' || ShipTypeN(tmp2)==t_Pirate && !Rnd(0,2) || ShipType(tmp2) == 'Smuggler') {",
              "                        tmp3=CreateQuestItem('Usl_35',Fei); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                }",
              "                //Гаальцы",
              "                if(ShipPilotRace(tmp2)==4)",
              "                {",
              "                    if(ShipType(tmp2) == 'Ranger' || Rnd(0,1) && ShipTypeN(tmp2)==t_Warrior || ShipType(tmp2) == 'Diplomat')",
              "                    {",
              "                        tmp3=CreateQuestItem('Usl_56',Gaal); ArrayAdd(hit_list,tmp3);",
              "                    }",
              "                }",
              "",
              "                if(ShipType(tmp2) == 'Ranger' && Rnd(0,3) || ShipType(tmp2) == 'RangerPirate' && !Rnd(0,2) || ShipType(tmp2) == 'Liner' && Rnd(0,2))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_58',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                tmp3=CreateQuestItem('Usl_63',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                if(ShipType(tmp2) == 'RangerPirate' || Rnd(0,1) && ShipTypeN(tmp2)==t_Pirate )",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_71',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                tmp3=CreateQuestItem('Usl_72',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                if(ShipType(tmp2)!='Warrior') {",
              "                    tmp3=CreateQuestItem('Usl_74',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if (ShipType(tmp2) == 'Ranger' && !Rnd(0,1) || ShipType(tmp2) == 'Diplomat' || ShipTypeN(tmp2)==t_Pirate && GetShipPirateRank(tmp2)>3)",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_DataArchive',None); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipType(tmp2) == 'Ranger' && Rnd(0,3) || ShipType(tmp2) == 'Transport' && Rnd(0,1) || ShipType(tmp2) == 'Smuggler' || ShipType(tmp2) == 'RangerPirate' && Rnd(0,2) || ShipTypeN(tmp2)==t_Pirate && !Rnd(0,2)) {",
              "                    tmp3=CreateQuestItem('Usl_Faruk',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if (ShipType(tmp2) == 'Liner' && !Rnd(0,2) || ShipType(tmp2) == 'Smuggler' && !Rnd(0,2) || ShipType(tmp2) == 'Ranger' && !Rnd(0,2))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_LasosCup',None); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if (ShipTypeN(tmp2)==t_Pirate && !Rnd(0,2) && ShipOwner(tmp2)==PirateClan || ShipType(tmp2) == 'RangerPirate' && Rnd(0,1))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_MemoryModule',None); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if (ShipType(tmp2) == 'Liner' && !Rnd(0,4) || ShipTypeN(tmp2)==t_Warrior && !Rnd(0,2) || ShipType(tmp2) == 'Diplomat' || ShipType(tmp2) == 'Ranger' || ShipType(tmp2) == 'RangerPirate' || ShipType(tmp2) == 'Smuggler')",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_PTQ68',None); ArrayAdd(hit_list,tmp3);",
              "                    tmp3=CreateQuestItem('Usl_PTQn60',None); ArrayAdd(hit_list,tmp3);",
              "                    tmp3=CreateQuestItem('Usl_PTQn64',None); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipPilotRace(tmp2)==1 || ShipPilotRace(tmp2)!=1 && !Rnd(0,2) && ShipTypeN(tmp2)!=t_Warrior)",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_14',Peleng); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipTypeN(tmp2)!=t_Pirate && ShipTypeN(tmp2)!=t_Warrior && ShipPilotRace(tmp2)==1 || ShipPilotRace(tmp2)!=1 && !Rnd(0,4) && ShipTypeN(tmp2)!=t_Pirate && ShipTypeN(tmp2)!=t_Warrior)",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_18',Peleng); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if((ShipType(tmp2) == 'Ranger' || ShipType(tmp2) == 'RangerPirate' || ShipType(tmp2) == 'Diplomat') && !Rnd(0,2))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_23',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipTypeN(tmp2)!=t_Warrior && ShipType(tmp2)!='Diplomat')",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_37',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if((ShipType(tmp2) == 'Diplomat' || ShipType(tmp2) == 'Ranger') && Rnd(0,1) || ShipPilotRace(tmp2)==1 && (ShipType(tmp2) == 'Pirate' || ShipTypeN(tmp2)==t_Ranger || ShipTypeN(tmp2)==t_Transport))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_40',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipTypeN(tmp2)==t_Ranger && !Rnd(0,2) && ShipPilotRace(tmp2)!=1 || ShipPilotRace(tmp2)==1 && ShipType(tmp2) == 'Ranger' && Rnd(0,1)) {",
              "                    tmp3=CreateQuestItem('Usl_46',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if((ShipType(tmp2) == 'Diplomat' || ShipType(tmp2) == 'Ranger') && !Rnd(0,2) && ShipPilotRace(tmp2) || !ShipPilotRace(tmp2) && Rnd(0,1) && (ShipType(tmp2) == 'Diplomat' || ShipType(tmp2) == 'Ranger'))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_40',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipTypeN(tmp2)!=t_Warrior)",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_8',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipTypeN(tmp2)!=t_Warrior && ShipPilotRace(tmp2) && ShipPilotRace(tmp2)!=2 && Rnd(0,2) || ShipType(tmp2)!=t_Warrior && ShipType(tmp2)!=t_Pirate && !Rnd(0,2) && ShipPilotRace(tmp2)!=1 && ShipPilotRace(tmp2)!=2)",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_Cactus',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Smuggler' || Rnd(0,1) && (ShipType(tmp2) == 'Liner' || ShipType(tmp2) == 'Diplomat') || !Rnd(0,2) && ShipType(tmp2) == 'Ranger')",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_Pilot',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                if(!ShipPilotRace(tmp2) || ShipPilotRace(tmp2)==3 && !Rnd(0,2) || ShipPilotRace(tmp2)!=0 && ShipPilotRace(tmp2)!=3 && Rnd(0,1))",
              "                {",
              "                    tmp3=CreateQuestItem('Usl_Svarokok',ShipOwner(tmp2)); ArrayAdd(hit_list,tmp3);",
              "                }",
              "                //Из сформированного массива рандомно выбирается один предмет",
              "                if(ArrayDim(hit_list)>2) tmp3=hit_list[Rnd(1,ArrayDim(hit_list)-1)];",
              "                else tmp3=hit_list[1];",
              "                ShipAddDropItem(tmp2,tmp3);",
              "                ArrayClear(hit_list);",
              "            }",
              "",
              "            //С шансом 1:35 после уничтожения корабля останется гиперпыль",
              "            if(!Rnd(0, 35))",
              "            {",
              "                tmp3 = CreateQuestItem('Dust', None);",
              "                ShipAddDropItem(tmp2, tmp3);",
              "            }",
              "",
              "            //Раздача спасательных капсул",
              "            if(ShipInScript(tmp2) /*&& ShipType(tmp2) != 'RangerPirate' && ShipType(tmp2) != 'Smuggler'*/) continue;",
              "",
              "            chance = 0;",
              "            if(ShipSpecialBonuses(tmp2, bonHookRadius) == 1) chance = 0;",
              "            else if(ShipType(tmp2) == 'Ranger')",
              "            {",
              "                type=1;",
              "                chance=40;",
              "            }",
              "            else if (ShipType(tmp2) == 'RangerPirate')",
              "            {",
              "                type=2;",
              "                chance=40;",
              "            }",
              "            else if(ShipType(tmp2) == 'Diplomat')",
              "            {",
              "                type=3;",
              "                chance=35;",
              "            }",
              "            else if(ShipType(tmp2) == 'Transport' || ShipType(tmp2) == 'Smuggler')",
              "            {",
              "                type=4;",
              "                chance=30;",
              "            }",
              "            else if(ShipType(tmp2) == 'Liner')",
              "            {",
              "                type=5;",
              "                chance=30;",
              "            }",
              "            else if(ShipTypeN(tmp2)==t_Pirate && !PirateType(tmp2))",
              "            {",
              "                type=2;",
              "                chance=35;",
              "            }",
              "            else if(ShipTypeN(tmp2)==t_Pirate && PirateType(tmp2))",
              "            {",
              "                type=2;",
              "                chance=25;",
              "            }",
              "",
              "            if(Rnd(1, 100) <= chance)",
              "            {",
              "                tmp3 = CreateQuestItem('Usl_EscapePod', ShipPilotRace(tmp2));",
              "                SetName(tmp3, ShortName(tmp2));",
              "                if(type == 1) UselessItemText(tmp3, \"Спасательная капсула, в которой лежит рейнджер <ShortName(tmp2)>. Необходимо доставить в ближайший медицинский центр.\");",
