              "            if(EtherState('ExpBK_TradeNewsEther') == -1) last_trade_news_upd = CurTurn();",
              "        ",
              "            temp_str = \"<color=255,240,100>Свежие экономические новости:</color>\" + \"<br>\" + ' ' + \"<br>\";",
              "            temp_int = 0;",
              "            cnt = PlanetNews() - 7;",
              "            for(i = PlanetNews() - 1; i > cnt; i = i - 1)",
