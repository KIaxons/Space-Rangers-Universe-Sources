          "#":  55,
          "Total.Lines":  39,
          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "dword ruin = GetShipRuins(Player());",
              "if(ruin)",
              "{",
              "    if(ShipType(ruin) == 'PB')",
              "    {",
              "        if(!GotoGov())",
              "        {",
              "            if(!ShipInPirateClan(Player()))",
              "            {",
              "                int check = 0;",
              "                if(CurrentMods('Tweaks\\RevolutionDelayed'))",
              "                {",
              "                    if(GetValueFromScript('Mod_RevolutionDelayed', 'pirates_captured_first_star')) check = 1;",
              "                }",
              "                else check = 1;",
              "",
              "                if(check)",
              "                {",
              "                    if(~StatusPlayer()) //Если игрок по характеру не пират",
              "                    {",
              "                        AddDialogInject('DialogPB', \"Кроме того, мы можем прямо сейчас сделать тебе одно особое предложение, от которого крайне сложно будет отказаться...\", \"Что за особое предложение?\", 1);",
              "                    }",
              "                    else //Если пират",
              "                    {",
              "                        AddDialogInject('', \"Кого еще тут принесла нелегкая? А, это ты, брателло? Приветствую тебя на нашей пиратской...\", '', 10, 1);",
              "                        AddDialogInject('DialogPirate', \"А чего это тебя в списках нет? Видно опять техническая группа что-то накосячила. Ладно, дело поправимое.  В общем так, <ShipName(Player())>, вот тебе уже заполненное заявление на прием в братство пиратов. Как будешь готов, просто поставь крестик или галочку там внизу и будем считать, что все в ажуре.\", \"Вступить в братство пиратов\", 9, 0);",
              "",
              "                        //У игрока появилась возможность приобрести саб-модем",
              "                        if(PlayerMayTakeSubCrack())",
              "                        {",
              "                            AddDialogInject('', \"Кстати, сегодня мы готовы продать тебе кое-что интересное. Но только если у тебя есть на счету <SubCrackCost()> cr.\", '', 8, 0);",
              "                        }",
              "                    }",
              "                }",
              "            }",
              "        }",
              "        else AddDialogOverride('DialogJoinLock', 20000);",
              "    }",
              "}"          
          ]        
        }      
