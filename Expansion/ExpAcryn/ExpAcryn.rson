              "ItemSize(tdwd, RndObject(10, 18, GetTalkShip()));",
              "LinkItemToScript(tdwd, Q_item);",
              "if(ItemOnUseCode(tdwd) != '') ItemOnUseCode(tdwd, '');",
              "AddItemToShip(Player(), tdwd);",
              "",
              "//Разрешаем продажу камуфляжей на флагманах",
              "if(!cham) cham = 1;",
              "",
              "Ether(3, 'ExpAcryn_quest_0', CT('Script.Mod_ExpAcryn.MissionIsActive') + CT('Script.Mod_ExpAcryn.MalocMission'));",
              "if(RelationToRanger(GetShipHomePlanet(flagman), Player()) <= 60) RelationToRanger(GetShipHomePlanet(flagman), Player(), RelationToRanger(GetShipHomePlanet(flagman), Player()) + 40);",
              "else RelationToRanger(GetShipHomePlanet(flagman), Player(), 100);",
