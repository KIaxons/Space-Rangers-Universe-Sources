          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "dword talk_ship = GetTalkShip();",
              "if(talk_ship)",
              "{",
              "    if(GExpImplantsDialog)",
              "    {",
              "        //Диалог с попыткой применить \"Вспышку\" или \"Фобос\" на игрока",
              "        if(GExpImplantsDialog == 1)",
              "        {",
              "            temp_int_1 = ShipFindCustomShipInfoByType(talk_ship, 'bioMM_FL_NPC');",
              "            temp_str = '</color>' + ShipCustomShipInfoTextData(talk_ship, temp_int_1, 1);",
              "            temp_str_1 = '</color>' + ShipCustomShipInfoTextData(talk_ship, temp_int_1, 2);",
              "            AddDialogOverride('BlindOnPlayer', 125000); //Так много на случай, если у скриптовых пиратов окажутся свои оверрайды",
              "        }",
              "        else if(GExpImplantsDialog == 2)",
              "        {",
              "            temp_int_1 = ShipFindCustomShipInfoByType(talk_ship, 'bioMM_FS_NPC');",
              "            temp_str = '</color>' + ShipCustomShipInfoTextData(talk_ship, temp_int_1, 1);",
              "            temp_str_1 = '</color>' + ShipCustomShipInfoTextData(talk_ship, temp_int_1, 2);",
              "            AddDialogOverride('FearOnPlayer', 125000); //Так много на случай, если у скриптовых пиратов окажутся свои оверрайды",
              "        }",
              "",
              "        GExpImplantsDialog = 0;",
              "        exit;",
              "    }",
              "",
              "    if(!TalkByAI())",
              "    {",
              "        temp_str = ShipType(talk_ship);",
              "        if(ShipTypeN(talk_ship) && ShipTypeN(talk_ship) <= t_Warrior)",
              "        {",
              "            if(ShipOwner(talk_ship) != None)",
              "            {",
              "                if(add_dialog[2] >= 1)",
              "                {",
              "                    if(add_dialog_cd[2] < CurTurn())",
              "                    {",
              "                        temp_int_1 = 0;",
              "                        //Исключение флагманов",
              "                        if(ShipTypeN(talk_ship) == t_Warrior && ShipSubType(talk_ship) == 1) temp_int_1 = 1;",
              "                        if(!temp_int_1)",
              "                        {",
              "                            //Если корабль ещё не ослеплён",
              "                            if(!ShipHealthFactor(talk_ship, 1))",
              "                            {",
              "                                //Добавляем диалог для активации \"Вспышки\"",
              "                                temp_int_1 = CT('Biomodules.1_gtu.9.BlindStart.Text_count');",
              "                                temp_str = '</color>' + CT('Biomodules.1_gtu.9.BlindStart.Text_' + RndObject(1, temp_int_1, talk_ship)) + ' ' + CT('Biomodules.1_gtu.9.PlayerText');",
              "                                AddDialogInject('dialog_blind', '', temp_str, 1);",
              "                            }",
              "                        }",
              "                    }",
              "                }",
              "    ",
              "                if(add_dialog[1] >= 1)",
              "                {",
              "                    if(add_dialog_cd[1] < CurTurn())",
              "                    {",
              "                        temp_int_1 = 0;",
              "                        if(ShipTypeN(talk_ship) == t_Warrior)",
              "                        {",
              "                            //Исключение флагманов",
              "                            if(ShipSubType(talk_ship) == 1) temp_int_1 = 1;",
              "                        }",
              "                        ",
              "                        if(!temp_int_1)",
              "                        {",
              "                            //Добавляем диалог для активации \"Фобоса\"",
              "                            temp_int_1 = CT('Biomodules.8_gtu.9.FearStart.Text_count');",
              "                            temp_str = '</color>' + CT('Biomodules.8_gtu.9.FearStart.Text_' + RndObject(1, temp_int_1, talk_ship)) + ' ' + CT('Biomodules.8_gtu.9.PlayerText');",
              "                            AddDialogInject('dialog_hypno', '', temp_str, 1);",
              "                        }",
              "                    }",
              "                }",
              "            }",
              "        }",
              "    }",
              "}"          
          ]        
        }      
