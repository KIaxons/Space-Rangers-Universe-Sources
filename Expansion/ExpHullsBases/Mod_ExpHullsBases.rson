          "Parent":  5,
          "#":  464,
          "Total.Lines":  145,
          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "cur_ruin = GetShipRuins(Player());",
              "if(cur_ruin)",
              "{",
              "    //Если игрок не на мостике",
              "    if(!PlayerBridge())",
              "    {",
              "        ruin_str_type = Format(ShipType(cur_ruin), '_licensed', '', '');",
              "",
              "        cur_star = ShipStar(cur_ruin);",
              "",
              "        //Если игрок находится на военной базе",
              "        if(ruin_str_type == 'WB')",
              "        {",
              "            //Добавляем диалог для приказа смены дислокации ВБ (игрока могут позже послать, если его звание недостаточно высоко)",
              "            if(!StarOwner(cur_star))",
              "            {",
              "                if(!ShipOnSidePirateClan(Player()))",
              "                {",
              "                    if(ShipTurnBeforeEndOrder(cur_ruin) - 1 > 1) TextCorr3 = '</color>' + \"суток\";",
              "                    else TextCorr3 = '</color>' + \"сутки\";",
              "",
              "                    if(~ShipFindCustomShipInfoByType(cur_ruin, 'WB_move')) AddDialogInject('ruin_movement', \"Также напоминаю, что прямо сейчас наша база совершает передислокацию в пределах системы <StarName(cur_star)>. Мы движемся со скоростью <ShipSpeed(cur_ruin)> млн. км./день и должны достигнуть места назначения через <ShipTurnBeforeEndOrder(cur_ruin)> <TextCorr3>.\",\"Управление движением базы\", 0);",
              "                    else AddDialogInject('ruin_movement', '', \"Ваша база должна сменить дислокацию\", 1);",
              "                }",
              "            }",
              "            else",
              "            {",
              "                AddDialogOverride('OnSystemAttack', 1000);",
              "            }",
              "",
              "            if(ShipOwner(Player()) <= 4)",
              "            {",
              "                //Добавляем стандартные диалоги для улучшения базы",
              "                if(RuinsAllowModernization(cur_ruin))",
              "                {",
              "                    if(ruin_str_type != 'PB' && ruin_str_type != 'CB')",
              "                    {",
              "                        AddDialogInject('ruinhull_upgrade_talk', '', \"Я желаю улучшить корпус вашей станции\", 0);",
              "                    }",
              "",
              "                    AddDialogInject('ruin_modernization_negation', '', \"Я прекращаю спонсирование этой станции\", 0);",
              "                }",
              "                else",
              "                {",
              "                    AddDialogInject('ruinhull_upgrade_talk', '', \"Мне нужен подробный отчёт о состоянии станции\", 0);",
              "                }",
              "            }",
              "            //Если игрок на стороне клана, то улучшить ВБ он не сможет",
              "            else if(ShipOwner(Player()) == 7)",
              "            {",
              "                AddDialogBlock(CT('FormRuins.GN.Modern.PlayerAsk'), 2);",
              "                RuinsAllowModernization(cur_ruin, 0);",
              "            }",
              "        }",
              "        //Если игрок находится на какой-то другой станции",
              "        else if(",
              "                ruin_str_type == 'SB' ||",
              "                ruin_str_type == 'RC' ||",
              "                ruin_str_type == 'MC' ||",
              "                ruin_str_type == 'BK' ||",
              "                ruin_str_type == 'TC' ||",
              "                ruin_str_type == 'MB' ||",
              "                ruin_str_type == 'Manaan' ||",
              "                ruin_str_type == 'PB' ||",
              "                ruin_str_type == 'CB'",
              "            )",
              "        {",
              "            //Добавляем диалог в случае нахождения станции в гипере (и если игрок в этот момент сидит на ней же)",
              "            if(ShipInHyperSpace(cur_ruin)) AddDialogOverride('Hyper', 3000);",
              "            if(jumpCheck != '') AddDialogOverride(jumpCheck, 3001);",
              "",
              "            //Добавляем диалог для использования портального генератора (если он уже установлен на базе)",
              "            if(~ShipFindCustomShipInfoByType(cur_ruin, 'Teleport_Generator')) AddDialogInject('TeleportRun', '', \"Я хочу воспользоваться возможностями вашего портального генератора\", 0);",
              "",
              "            //Добавляем стандартные диалоги для улучшения базы",
              "            if(RuinsAllowModernization(cur_ruin))",
              "            {",
              "                if(ruin_str_type != 'PB' && ruin_str_type != 'CB') AddDialogInject('ruinhull_upgrade_talk', '', \"Я желаю улучшить корпус вашей станции\", 0);",
              "                AddDialogInject('ruin_modernization_negation', '', \"Я прекращаю спонсирование этой станции\", 0);",
              "            }",
              "            else",
              "            {",
              "                if(ruin_str_type != 'PB' && ruin_str_type != 'CB') AddDialogInject('ruinhull_upgrade_talk', '', \"Мне нужен подробный отчёт о состоянии станции\", 0);",
              "            }",
              "        }",
              "        else if(ruin_str_type == 'PlayerMB')",
              "        {",
              "            if(RuinsAllowModernization(cur_ruin))",
              "            {",
              "                //Если игрок повысил уровень штаба, но пока что не прожал \"Отлично!\" (т.к. этот ответ как раз и повышает уровень)",
              "                if(ruin_str_type == 'PlayerMB' && GotoGov())",
              "                {",
              "                    AddDialogOverride('RuinImproveDone', 4000, 10);",
              "                }",
              "            }",
              "        }",
              "    }",
              "    //Если игрок на мостике",
              "    else",
              "    {",
              "        if(PlayerBridge() == 1 || PlayerBridge() == 2 || PlayerBridge() == 3)",
              "        {",
              "            if(ShipInNormalSpace(Player()))",
              "            {",
              "                //Добавляем диалоги на мостик",
              "                cur_star = ShipStar(Player());",
              "",
              "                if(jumpCheck != '')",
              "                {",
              "                    AddDialogOverride(jumpCheck, 3001);",
              "                }",
              "                else",
              "                {",
              "                    for(i = 0; i < StarRuins(cur_star); i = i + 1)",
              "                    {",
              "                        cur_ruin = StarRuins(cur_star, i);",
              "                        if(!ShipInNormalSpace(cur_ruin)) continue;",
              "                        if(Dist(Player(), cur_ruin) > ShipRadarRange(Player())) continue;",
              "",
              "                        ruin_str_type = ShipType(cur_ruin);",
              "                        if(ruin_str_type == 'WB')",
              "                        {",
              "                            AddDialogInject('bridge_ruin_control', '', \"Связаться с <color=0,50,200>военной базой</color> <ShipName(cur_ruin)>\", 5);",
              "                        }",
              "                        else if(ruin_str_type == 'SB' || ruin_str_type == 'RC' || ruin_str_type == 'MC' || ruin_str_type == 'BK' || ruin_str_type == 'TC')",
              "                        {",
              "                            if(~ShipFindCustomShipInfoByType(cur_ruin, 'Teleport_Generator'))",
              "                            {",
              "                                if(ShipOrder(cur_ruin) != 7)",
              "                                {",
              "                                    if(ruin_str_type == 'SB') AddDialogInject('TeleportBridge', '', \"Связаться с <color=0,50,200>научной базой</color> <ShipName(cur_ruin)>\", 0, 0, cur_ruin);",
              "                                    else if(ruin_str_type == 'RC') AddDialogInject('TeleportBridge', '', \"Связаться с <color=0,50,200>центром рейнджеров</color> <ShipName(cur_ruin)>\", 0, 0, cur_ruin);",
              "                                    else if(ruin_str_type == 'MC') AddDialogInject('TeleportBridge', '', \"Связаться с <color=0,50,200>медицинским центром</color> <ShipName(cur_ruin)>\", 0, 0, cur_ruin);",
              "                                    else if(ruin_str_type == 'BK') AddDialogInject('TeleportBridge', '', \"Связаться с <color=0,50,200>бизнес-центром</color> <ShipName(cur_ruin)>\", 0, 0, cur_ruin);",
              "                                    else if(ruin_str_type == 'TC') AddDialogInject('TeleportBridge', '', \"Связаться с <color=0,50,200>технологическим центром</color> <ShipName(cur_ruin)>\", 0, 0, cur_ruin);",
              "                                }",
              "                            }",
              "                        }",
              "                    }",
              "                }",
              "                cur_ruin = 0;",
              "            }",
              "        }",
              "    }",
              "}"          
          ]        
        }      
