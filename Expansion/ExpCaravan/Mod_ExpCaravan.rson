              "        if(!(GetData(1, CurShip) & (1 << l))) continue;",
              "",
              "        int shot_permission = 1;",
              "        for(j = 0; j < GroupCount(Warriors); j = j + 1)",
              "        {",
              "            ship = GroupShip(Warriors, j);",
              "            int num = GetData(0, ship);",
