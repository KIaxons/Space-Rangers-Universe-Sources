              "if(!ItemIn(Cargo_1, ShipStar(CurShip)) && !ItemIn(Cargo_2, ShipStar(CurShip)) && !ItemIn(Cargo_3, ShipStar(CurShip)))",
              "{",
              "    if(ShipStar(CurShip) == PlayerStar) ChangeState('W_Home');",
              "    else ChangeState('W_End');",
              "    exit;",
              "}"          
          ]        
