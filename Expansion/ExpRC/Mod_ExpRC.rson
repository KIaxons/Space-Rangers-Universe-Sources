              "                else if(int1 > 9999 && int1 < 100000) str3 = 'td_margin_cost_00000';",
              "                else if(int1 > 99999 && int1 < 1000000) str3 = 'td_margin_cost_000000';",
              "                else if(int1 > 999999 && int1 < 10000000) str3 = 'td_margin_cost_0000000';",
