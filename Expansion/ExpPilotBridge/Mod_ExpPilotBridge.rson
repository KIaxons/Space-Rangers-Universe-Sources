              "                     '<KillFaeys'+'>', temp_str + GMercFeiStat,",
              "                     '<KillFreelancers'+'>', temp_str + GMercFreeStat,",
              "                     '<KillOtherMercs'+'>', temp_str + GMercOtherStat,",
              "                     '<KillMercenaryCenters'+'>', temp_str + G_MercCStat",
              "                    );",
              "}",
              "else temp_int_6 = GMercFreeStat; //Считаем встроенных в мод мостика фрилансеров как простых рейнджеров, если ShuMercs не подключён",
              "",
              "//Проверка на подключение мода ShuDomiks (верфи, ратторы и ингибиторы)",
              "LDomRatStat = '';",
              "L_IngBStat = '';",
              "L_DomBStat = '';",
              "if(CurrentMods('ShusRangers\\ShuDomiks'))",
              "{",
              "    LDomRatStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsDomikRattors'), '<KillRattors'+'>', temp_str + GDomRatStat);",
              "    L_IngBStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsInhibitors'), '<KillInhibitors'+'>', temp_str + G_IngBStat);",
              "    L_DomBStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsDomikShipyards'), '<KillDomikShipyards'+'>', temp_str + G_DomBStat);",
              "}",
              "",
              "//Проверка на подключение мода ShuKlissan (клисане и нейролинки)",
              "LKlissanStat = '';",
              "LKlissanLine = '<clr><clrEnd>'; //Если ShuKlissan не подключён, здесь будет пропуск строки",
              "if(CurrentMods('ShusRangers\\ShuKlissan'))",
              "{",
              "    LKlissanStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.TotalStatsKillKlissan'), '<KillKlissan'+'>', temp_str + GKlissanStat);",
              "    LKlissanLine = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsKlissan'),",
              "                     '<KillMutonies'+'>', temp_str + GKlissMutStat,",
              "                     '<KillMonchiks'+'>', temp_str + GKlissRogStat,",
              "                     '<KillCatauris'+'>', temp_str + GKlissKatStat,",
              "                     '<KillNonduses'+'>', temp_str + GKlissNonStat,",
              "                     '<KillEgemons'+'>', temp_str + GKlissEgeStat,",
              "                     '<KillGigases'+'>', temp_str + GKlissGigStat,",
              "                     '<KillNeurolinks'+'>', temp_str + G_KBStat",
              "                   );",
              "}",
              "",
              "//Проверка на подключение мода ExpTC (технологические центры)",
              "L_TCStat = '';",
              "if(CurrentMods('Expansion\\ExpTC'))",
              "{",
              "    L_TCStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsTechnologicalCenters'), '<KillTechnologicalCenters'+'>', temp_str + G_TCStat);",
              "}",
              "",
              "LArmWarStat = '';",
              "LArmPirStat = '';",
              "if(CurrentMods('Evolution\\EvoArmada'))",
              "{",
              "    LArmWarStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsDreadnoughts'), '<KillDreadnoughts'+'>', temp_str + GWarDreadStat);",
              "    LArmPirStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsPirateCapitals'),",
              "                     '<KillCorvettes'+'>', temp_str + GPirCorvStat,",
              "                     '<KillFrigates'+'>', temp_str + GPirFrigStat,",
              "                     '<KillGalleons'+'>', temp_str + GPirGallStat",
              "                    );",
              "}",
              "",
              "LPirSpecStat = '';",
              "if(CurrentMods('ShusRangers\\ShuPirates'))",
              "{",
              "    LPirSpecStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.DetailedShipsStatsPirateSpecials'),",
              "                     '<KillThugs'+'>', temp_str + GPirThugStat,",
              "                     '<KillKhans'+'>', temp_str + GPirKhanStat",
              "                   );",
              "}",
              "",
              "//Графа \"Прочих: [станций]\" на основном экране статистики",
              "//Полностью пропадает если ни один из модов: Mod_ShuDomiks, ShuKlissan и ShuMercs не подключён",
              "L_OtherStStat = '';",
              "if(CurrentMods('ShusRangers\\ShuDomiks') || CurrentMods('ShusRangers\\ShuKlissan') || CurrentMods('ShusRangers\\ShuMercs'))",
              "{",
              "    L_OtherStStat = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.TotalStatsKillOtherStations'), '<KillOtherStations'+'>', temp_str + (G_DomBStat + G_KBStat + G_MercCStat));",
              "}",
              "",
              "//Сборка готового текста в диалоговом окне",
              "tstr1 = '</color>' + Format(CT('Script.Mod_ExpPilotBridge.PersonalStatistic.TotalStats'),",
              "                     '<KillDomiks'+'>', temp_str + GDomStat,",
              "                     '<KillKlissan'+'>', LKlissanStat,",
              "                     '<KillPirates'+'>', temp_str + (GPirStat + ShipStatistic(Player(), 8)), //В зачёт также идут гиперпираты",
              "                     '<KillMercs'+'>', LMercStat,",
              "                     '<KillRangers'+'>', temp_str + (GRangStat + GRangSpecStat + temp_int_6),",
              "                     '<KillWarriors'+'>', temp_str + GWarStat,",
              "                     '<KillCivils'+'>', temp_str + GCivilStat,",
              "                     '<KillOthers'+'>', temp_str + (GTrankStat + GUnknStat + ShipStatistic(Player(), 7) + ShipStatistic(Player(), 8)),",
              "                     '<KillCoalStations'+'>', temp_str + (G_MBStat + G_RCStat + G_SBStat + G_BCStat + G_MCStat + G_TCStat + G_IngBStat),",
              "                     '<KillPirateStations'+'>', temp_str + (G_PCBStat + G_PBStat),",
              "                     '<KillOtherStations'+'>', L_OtherStStat,",
              "                     '<FreeStarsForCoal'+'>', temp_str + sys_coal,",
              "                     '<FreeStarsForPirates'+'>', temp_str + sys_pirate,",
              "                     '<TextQuestsCompleted'+'>', temp_str + txt_q_ok,",
              "                     '<DeliveryQuestsCompleted'+'>', temp_str + SendQuestsOK,",
              "                     '<GuardQuestsCompleted'+'>', temp_str + DefShipQuestsOK,",
              "                     '<KillQuestsCompleted'+'>', temp_str + (KillQuestsOK + GCustomQuestsDone),",
              "                     '<StarDefenceQuestsCompleted'+'>', temp_str + DefSysQuestsOK,",
              "                     '<PlanetBattlesCompleted'+'>', temp_str + plan_bat,",
              "                     '<TextQuestsFailed'+'>', temp_str + TextQuestsFailed,",
              "                     '<DeliveryQuestsFailed'+'>', temp_str + SendQuestsFailed,",
              "                     '<ShipGuardQuestsFailed'+'>', temp_str + DefShipQuestsFailed,",
              "                     '<KillQuestsFailed'+'>', temp_str + (KillQuestsFailed + GCustomQuestsFailed),",
              "                     '<StarGuardQuestsFailed'+'>', temp_str + DefSysQuestsFailed,",
              "                     '<GoodsBoughtTotal'+'>', temp_str + money_buys,",
              "                     '<GoodsSellTotal'+'>', temp_str + money_sale,",
              "                     '<CurrentTradeLoss'+'>', temp_str + ShipStatistic(Player(), 9),",
              "                     '<EquipmentBoughtTotal'+'>', temp_str + money_equip,",
              "                     '<RacketeeringReceived'+'>', temp_str + money_take,",
              "                     '<RansomsReceived'+'>', temp_str + money_take_peace,",
              "                     '<QuestsReward'+'>', temp_str + money_quest,",
              "                     '<PlanetBattlesReward'+'>', temp_str + pb_money,",
              "                     '<ExpForKillDomiks'+'>', temp_str + GetShipExpByType(Player(), 1),",
              "                     '<ExpForKillPirates'+'>', temp_str + GetShipExpByType(Player(), 2),",
              "                     '<ExpForKillOthers'+'>', temp_str + GetShipExpByType(Player(), 3),",
              "                     '<ExpForTradeGoods'+'>', temp_str + GetShipExpByType(Player(), 4),",
              "                     '<ExpForQuests'+'>', temp_str + exp_quest,",
              "                     '<ExpForPlanetBattles'+'>', temp_str + pb_exp",
              "                   );"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  0,
          "Pos.y":  20,
          "Parent":  648,
          "#":  59,
          "Total.Lines":  763,
          "Code.Type":  "Turn",
          "Code":          
          [
              "//Запоминаем ход срабатывания терна, чтобы избежать лишних срабатываний за ход (которые случаются)",
              "//Нельзя делать это в предыдущем Op, т.к. тогда не будет работать автосрабатывание терна после завершения скриптового квеста",
              "lastTurn = CurTurn();",
              "",
              "//Для удобства сразу запоминаем текущий ГТУ в отдельную переменную",
              "GTL = GalaxyTechLevel();",
              "",
              "//Проверка на появление игрока в системе с Уитли (срабатывает один раз за игровую партию)",
              "if(GScriptWheatley == -1)",
              "{",
              "    if(ShipStar(Player()) == GalaxyStar(ScriptItemData(Script_Wheatley, 3))) GScriptWheatley = 1;",
              "}",
              "",
              "//Смена отношения на враждебное с планетой, с которой игрок совершил побег",
              "if(GKillQuestCheck == -1)",
              "{",
              "    RelationToRanger(temp_dword_4, Player(), 0);",
              "    GKillQuestCheck = 0;",
              "}",
              "",
              "//Таймер и провал квеста на убийство подставившего игрока пирата",
              "if(GKillQuestCheck == 3)",
              "{",
              "    if(ETimeLeft > CurTurn())",
              "    {",
              "        CustomEther('QuestNormal', 3, 'CustomQuestKillBridge', \"<color=255,240,100>Задание в процессе выполнения</color>\" + CT('FormInfo.br') + \"До окончания задания осталось <color=255,240,100>\" + (ETimeLeft - CurTurn()) + \"</color> дней\" + CT('FormInfo.br') + \"Заказчик - планета <color=255,240,100>\" + PlanetName(IdToPlanet(EQuestPlanet)) + \"</color> (система <color=255,240,100>\" + StarName(PlanetToStar(IdToPlanet(EQuestPlanet))) + '</color>)' + CT('FormInfo.br') + ' ' + CT('FormInfo.br') + GEQuestText);",
              "    }",
              "    if(ETimeLeft <= CurTurn())",
              "    {",
              "        CustomEther('QuestCancel', 5, 'CustomQuestKillBridge', \"<color=255,0,0>Задание провалено</color>\" + CT('FormInfo.br') + \"Заказчик - планета <color=255,240,100>\" + PlanetName(IdToPlanet(EQuestPlanet)) + \"</color> (система <color=255,240,100>\" + StarName(PlanetToStar(IdToPlanet(EQuestPlanet))) + '</color>)' + CT('FormInfo.br') + ' ' + CT('FormInfo.br') + GEQuestText);",
              "        NewsAdd(\"Сообщение с планеты <color=255,240,100>\" + PlanetName(IdToPlanet(EQuestPlanet)) + '</color>.' + CT('FormInfo.br') + \"Очень жаль, но вы сорвали сроки выполнения нашего задания. Контракт более не имеет юридической силы. Сделка отменяется.\");",
              "",
              "        RelationToRanger(IdToPlanet(EQuestPlanet), Player(), min(10, RelationToRanger(IdToPlanet(EQuestPlanet), Player())));",
              "        NoTalkToShip(IdToShip(ETargetShip), 0);",
              "        GCustomQuestsFailed = GCustomQuestsFailed + 1;",
              "        ShipDeleteCustomShipInfo(IdToShip(ETargetShip), 'PlayerBridge_IllWisherInfo');",
              "",
              "        GEQuestText = '';",
              "        ETargetShip = 0;",
              "        EQuestPlanet = 0;",
              "        EQuestReward = 0;",
              "        EQuestExpReward = 0;",
              "        ETimeLeft = 0;",
              "        GKillQuestCheck = 0;",
              "    }",
              "}",
              "//Полное снятие преследования, если все корабли преследователей перебиты или выведены из группы",
              "if(GCardPursueCheck > 0)",
              "{",
              "    if(!GroupCount(DebtMG))",
              "    {",
              "        GShipJoinGlobal = 0;",
              "        CardDebt = 0;",
              "        GCardPursueCheck = 0;",
              "    }",
              "    else",
              "    {",
              "        for(i = 0; i < GroupCount(DebtMG); i = i + 1)",
              "        {",
              "            temp_dword_4 = GroupShip(DebtMG,i);",
              "            //Заправляем и чиним пиратов, которые оказались на планете к концу хода",
              "            if(GetShipPlanet(temp_dword_4) || GetShipRuins(temp_dword_4))",
              "            {",
              "                ShipRepairEq(temp_dword_4);",
              "                ShipRefuel(temp_dword_4);",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "//Узнаём, что станции назначения и/или отправки груза покинули свою текущую систему",
              "if(transfer_day >= CurTurn())",
              "{",
              "    if(LTargetType == 2 || LTargetType == 4)",
              "    {",
              "        if(LStationDExist != 1)",
              "        {",
              "            if(ShipInHyperSpace(IdToShip(point_3)))",
              "            {",
              "                //Станция назначения доставки груза покинула систему",
              "                LStationDExist = 2;",
              "                point_3_name = ShipName(IdToShip(point_3));",
              "            }",
              "        }",
              "        if(LStationSExist != 1)",
              "        {",
              "            if(ShipInHyperSpace(IdToShip(point_1_id)))",
              "            {",
              "                //Станция отправки груза покинула систему",
              "                LStationSExist = 2;",
              "                point_1_name = ShipName(IdToShip(point_1_id));",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "//Узнаём, что станции назначения и/или отправки корпуса покинули свою текущую систему",
              "if(HullDelTime >= CurTurn())",
              "{",
              "    if(HullFromType == 4 || HullEndType == 4)",
              "    {",
              "        if(HullEndStExist != 1)",
              "        {",
              "            if(ShipInHyperSpace(IdToShip(HullDelEndId)))",
              "            {",
              "                //Станция назначения доставки корпуса покинула систему",
              "                HullEndStExist = 2;",
              "                HullEndName = ShipName(IdToShip(HullDelEndId));",
              "            }",
              "        }",
              "        if(HullFromStExist != 1)",
              "        {",
              "            if(ShipInHyperSpace(IdToShip(HullDelFromId)))",
              "            {",
              "                //Станция отправки корпуса покинула систему",
              "                HullFromStExist = 2;",
              "                HullFromName = ShipName(IdToShip(HullDelFromId));",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "//Перебираем все галактические эвенты, совершаем необходимые действия",
              "for(j = GalaxyEvents() - 1; j >= 0; j = j - 1)",
              "{",
              "    if(GalaxyEventDate(j) < CurTurn()) break;",
              "    temp_str_4 = GalaxyEventType(j);",
              "    if(temp_str_4 == 'RuinsDestroyed')",
              "    {",
              "        //Узнаём, что станции назначения и/или отправки груза были уничтожены",
              "        if(transfer_day >= CurTurn())",
              "        {",
              "            if(LTargetType == 2 || LTargetType == 4)",
              "            {",
              "                if(GalaxyEventData(j, 1) == point_3)",
              "                {",
              "                    //Была уничтожена станция назначения доставки груза",
              "                    LStationDExist = 1; //Не смотрите на 1 - это значит как раз то, что станция уничтожена",
              "                    point_3_name = GalaxyEventsTextData(j, 0);",
              "                }",
              "                if(GalaxyEventData(j, 1) == point_1_id)",
              "                {",
              "                    //Была уничтожена станция отправки груза",
              "                    LStationSExist = 1;",
              "                    point_1_name = GalaxyEventsTextData(j, 0);",
              "                }",
              "            }",
              "        }",
              "",
              "        //Узнаём, что станции назначения и/или отправки корпуса были уничтожены",
              "        if(HullDelTime >= CurTurn())",
              "        {",
              "            if(HullFromType == 4 || HullEndType == 4)",
              "            {",
              "                if(GalaxyEventData(j, 1) == HullDelEndId)",
              "                {",
              "                    //Была уничтожена станция назначения доставки корпуса",
              "                    HullEndStExist = 1;",
              "                    HullEndName = GalaxyEventsTextData(j, 0);",
              "                }",
              "                if(GalaxyEventData(j, 1) == HullDelFromId)",
              "                {",
              "                    //Была уничтожена станция отправки корпуса",
              "                    HullFromStExist = 1;",
              "                    HullFromName = GalaxyEventsTextData(j, 0);",
              "                }",
              "            }",
              "        }",
              "",
              "        //Пересобираем массив с доставленными корпусами в случае уничтожения доставочной станции",
              "        if(ArrayDim(GHullsWaitArray) > 1)",
              "        {",
              "            temp_dword_1 = 0;",
              "            //Проверяем, была ли взорвана станция с корпусом",
              "            for(i = 1; i < ArrayDim(GHullsWaitArray); i = i + 1)",
              "            {",
              "                temp_dword=substr(GHullsWaitArray[i],findsubstr(GHullsWaitArray[i],',')+1,(findsubstr(GHullsWaitArray[i],'*')-findsubstr(GHullsWaitArray[i],',')-1));",
              "                if(GalaxyEventData(j,1)==temp_dword)",
              "                {",
              "                    if(ArrayDim(GHullsWaitArray)==2) ArrayClear(GHullsWaitArray);",
              "                    else",
              "                    {",
              "                        GHullsWaitArray[i]=-1;",
              "                        temp_dword_1=1;",
              "                    }",
              "                }",
              "            }",
              "            //Пересобираем массив, исключаем пропавшие со склада корпуса",
              "            if(temp_dword_1)",
              "            {",
              "                ArrayClear(GAddTArray2);",
              "                for(i=1; i<ArrayDim(GHullsWaitArray); i=i+1)",
              "                {",
              "                    if(GHullsWaitArray[i]!=-1) ArrayAdd(GAddTArray2,GHullsWaitArray[i]);",
              "                }",
              "                ArrayClear(GHullsWaitArray);",
              "                for(i=1; i<ArrayDim(GAddTArray2); i=i+1)",
              "                {",
              "                    ArrayAdd(GHullsWaitArray,GAddTArray2[i]);",
              "                }",
              "                ArrayClear(GAddTArray2);",
              "            }",
              "        }",
              "    }",
              "    //Счётчики уничтоженных кораблей",
              "    else if(temp_str_4 == 'PlayerKillsShip')",
              "    {",
              "        UpdateKillShipsStatistic(GalaxyEventData(j, 0), GalaxyEventData(j, 5), GalaxyEventsTextData(j, 2), GalaxyEventsTextData(j, 0), GalaxyEventsTextData(j, 1), GalaxyEventData(j, 3), 0);",
              "    }",
              "    //Счётчики уничтоженных кораблей в гипере",
              "    else if(temp_str_4 == 'LabeledShipKilledInAB')",
              "    {",
              "        tstr2 = GalaxyEventsTextData(j, 0);",
              "        if(ParsCountFromString(tstr2, ',') >= 6)",
              "        {",
              "            if(~findsubstr(GetParFromString(tstr2, ',', 2, 0), '_Friend')) continue; //Пропускаем уничтоженные в гипере, но дружественные по отношению к игроку корабли",
              "            UpdateKillShipsStatistic(GetParFromString(tstr2, ',', 0, 0), GetParFromString(tstr2, ',', 1, 0), GetParFromString(tstr2, ',', 2, 0), GetParFromString(tstr2, ',', 3, 0), GetParFromString(tstr2, ',', 4, 0), GetParFromString(tstr2, ',', 5, 0), 1);",
              "            ShipStatistic(Player(), 7, ShipStatistic(Player(), 7) - 1);",
              "        }",
              "    }",
              "    //Прочие события",
              "    else if(temp_str_4 == 'PlayerBuysEquipment') money_equip = money_equip + GalaxyEventData(j, 1);",
              "    else if(temp_str_4 == 'PlayerFinishesPlanetaryBattle')",
              "    {",
              "        pb_money = pb_money + GalaxyEventData(j, 1);",
              "        pb_exp = pb_exp + GalaxyEventData(j, 2);",
              "    }",
              "    else if(temp_str_4 == 'PlayerFinishesQuest')",
              "    {",
              "        money_quest = money_quest + GalaxyEventData(j, 2);",
              "        exp_quest = exp_quest + GalaxyEventData(j, 3);",
              "    }",
              "    else if(temp_str_4 == 'PlayerExtortsMoney')",
              "    {",
              "        temp_int_1 = GalaxyEventData(j, 0);",
              "        money_take = money_take + temp_int_1;",
              "    }",
              "    else if(temp_str_4 == 'PlayerAcceptsMoneyForTruce')",
              "    {",
              "        temp_int_1 = GalaxyEventData(j, 0);",
              "        money_take_peace = money_take_peace + temp_int_1;",
              "    }",
              "    else if(temp_str_4 == 'PlayerBuysGoodsFromPlanet')",
              "    {",
              "        temp_int_1 = GalaxyEventData(j, 2);",
              "        money_buys = money_buys + temp_int_1;",
              "    }",
              "    else if(temp_str_4 == 'PlayerSellsGoodsToPlanet')",
              "    {",
              "        temp_int_1 = GalaxyEventData(j, 2);",
              "        money_sale = money_sale + temp_int_1;",
              "    }",
              "    else if(temp_str_4 == 'PlayerLiberatesSystem')",
              "    {",
              "        if(GalaxyEventData(j, 1) == 0) sys_coal = sys_coal + 1;",
              "        if(GalaxyEventData(j, 1) == 2) sys_pirate = sys_pirate + 1;",
              "    }",
              "}",
              "",
              "//Обработчики прерывания доставки груза",
              "if(transfer_day > CurTurn())",
              "{",
              "    //Груз был в пути на планету отправки, но её захватили (груз перенаправлен в родную систему)",
              "    if(LTargetType==3)",
              "    {",
