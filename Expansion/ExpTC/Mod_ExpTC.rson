              "}",
              "if(ItemType(temp_dword) == t_DefGenerator)",
              "{",
              "    SetEquipmentStats(temp_dword,GetEquipmentStats(clon_item,0),0);",
              "}",
              "if(ItemType(temp_dword) >= t_Weapon1 && ItemType(temp_dword) <= t_CustomWeapon)",
