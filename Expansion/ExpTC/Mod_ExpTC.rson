          "Code":          
          [
              "ArrayClear(bh_items);",
              "ArrayClear(bh_item_names);",
              "for(i=0;i<ShipItems(Player());i=i+1)",
              "{",
              "    cur_bh_item=ShipItems(Player(),i);",
              "    if(cur_bh_item==temp_dword) continue;",
              "    if(ItemOwner(cur_bh_item)!=ItemOwner(temp_dword))continue;",
